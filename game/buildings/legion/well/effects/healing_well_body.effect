<?xml version="1.0" encoding="UTF-8"?>
<effect name="water_splash_01">
	<definitions>
		<particlesystem name="system0" space="world" scale="1">
		
	
		
		<template name="fountain">			

			<billboard
				bone="{bone}"
				color="0 .05 .061"
				startalpha="1.0"
				midalpha="1.0"
				endalpha="1.0"
				midalphapos="0.75"
				startparam=".05"
				endparam="0.00"
				
				size="368"
				depthbias="0"
				position="0 0 0"
				material="/shared/effects/materials/whitedot.material"
				lockup="true"
				pitch="90"
			/>
			
			<billboard
				bone="{mesh}"
