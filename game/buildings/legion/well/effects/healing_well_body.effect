				speed="200"
				drag="0"
				minangle="0"
				maxangle="10"
				position="0 0 0"
				particlelife="1200"
				material="/shared/effects/materials/refract_stream.material"			
			>
				<particle 
				param=".2"			
				startsize="5"
					endsize="33"				
					color="1 1 1"
					turn="1"
					startheightdistort=".5"
					endheightdesitort="0"
					
				/>
			</meshemitter>
			

			<meshemitter		
