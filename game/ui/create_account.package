<?xml version="1.0" encoding="UTF-8"?>
<package>
	<template name="field">
		<panel height="22" name="{widgetname}">
			<panel x="-20" width="230"  align="right">
				<frame noclick="true" texture="/ui/frames/rounded_bg_white.tga" borderthickness="6" color="black"/>
				<frame noclick="true" texture="/ui/frames/roundframe.tga" color="#FFDA00" borderthickness="6"/>
				<textbox x="5" y="4" taborder="{order}" passwordchar="{passchar}" textvalign="center" valign="center" font="chat_12" color="invisible" textcolor="white" shadow="true"  onchange="{cmd}(this);" onshow="{onshow}"/>
			</panel>					
			<panel noclick="true" x="-260"  align="right">
				<label noclick="true" height="100%" width="100%" textalign="right" textvalign="center" font="chat_13" shadow="true" color="{color}">{label}</label> 	
			</panel>				
		</panel>
	</template>
	
