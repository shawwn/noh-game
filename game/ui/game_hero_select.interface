	</template>

	<panel x="-30h" y="12h" width="20h" height="-16h" align="center" float="bottom" noclick="true" passivechildren="true">
		<instance name="player_tooltip_set" index="0" />
		<instance name="player_tooltip_set" index="1" />
		<instance name="player_tooltip_set" index="2" />
		<instance name="player_tooltip_set" index="3" />
		<instance name="player_tooltip_set" index="4" />
	</panel>

	<panel x="30h" y="12h" width="20h" height="-16h" align="center" float="bottom" noclick="true" passivechildren="true">
		<instance name="player_tooltip_set" index="5" />
		<instance name="player_tooltip_set" index="6" />
		<instance name="player_tooltip_set" index="7" />
		<instance name="player_tooltip_set" index="8" />
		<instance name="player_tooltip_set" index="9" />
	</panel>

	<!-- Random Tooltip -->
	<instance
		name="generic_tooltip" tip_id="random_hero_tip"
		y="6h"
		width="19h" height="6.0h"
		align="center"
		visible="false"
		title="hselect_button_ready"
		label_watch="HeroSelectInfo"
		label_ontrigger="
			If(param7, SetText(Translate('game_hero_select_random_tip_already_picked')),
			If((param11 == 0) or param12, SetText(Translate('game_hero_select_random_tip_free')),
			SetText(Translate('game_hero_select_random_tip', 'gold', param11))
			));"
	/>
	
	<!-- Arcade countdown -->
	<panel y="5h" width="80h" height="30h" align="center" noclick="true"
		watch="HeroSelectTimer"
		ontrigger="If(param3 and (param0 le 10000), PlaySound('/shared/sounds/ui/hero_select/button_clock.wav'));"
	>
		<modelpanel
			visible="false"
			noclick="true"

			model="common/models/arcade_text/countdown.mdf"
			anim="idle"
			camerapos="0 350 0"
			cameraangles="0 0 180"
			camerafov="50"
			cameranear="1"
			carmerafar="5000"
			depthclear="false"
			depthcompress="true"
			sunazimuth="140"
			sunaltitude="10"
			suncolor="1.4 1.3 1"

			onshow="SetAnim('idle'); SetEffect('/ui/common/models/arcade_text/countdown.effect');"
			watch="HeroSelectTimer"
			ontrigger="
					If(param0 gt 6000, SetVisible(false));
					If(param3 and (param0 le 6000), SetVisible(true));
					If(param3 and (param0 le 1000), PlaySound('/shared/sounds/announcer/startgame.wav'));
					"
		/>
	</panel>
</interface>
