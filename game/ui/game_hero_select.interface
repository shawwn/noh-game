			<instance
				name="generic_tooltip"
				tip_id="gold_tip{index}"
				height="6h"
				visible="false"
				title="game_hero_select_starting_gold"
				label_watch="HeroSelectPlayerInfo{index}"
				label_ontrigger="
					If(param1,
						SetText(Translate('game_hero_select_starting_gold_tip_me', 'gold', param17)),
						SetText(Translate('game_hero_select_starting_gold_tip_other', 'gold', param17))
