	<!-- Player Action Button Tooltips -->
	<template name="player_tooltip_set">
		<panel height="20%" noclick="true" passivechildren="true" watch="HeroSelectPlayerInfo{index}" ontrigger="SetVisible(param0 ge 0);" sticky="true" stickytoinvis="false">
