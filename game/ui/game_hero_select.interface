<?xml version="1.0" encoding="UTF-8"?>
<interface name="game_hero_select" scenex="0" sceney="0" scenewidth="100%" sceneheight="88h">

	<!-- Packages -->
	<include file="ui_items.package"/>
	<include file="game_chat.package"/>
	
	<!-- Dynamic Textures -->
	<texture file="/ui/elements/line_break.tga" />
	<texture file="/ui/elements/heroselection/player_action_ready_yes.tga" />
	<texture file="/ui/elements/heroselection/player_action_ready_no.tga" />
	<texture file="/ui/elements/heroselection/ready.tga" />
	<texture file="/ui/elements/heroselection/not_ready.tga" />
	
	<effect file="/ui/common/models/hero_select/hero_swap.effect" />
	<effect file="/ui/common/models/hero_select/hero_ready.effect" />
	<effect file="/ui/common/models/hero_select/hero_select.effect" />
	<effect file="/ui/common/models/arcade_text/countdown.effect" />

	<!-- Templates -->
	<template name="player_pick_random">
		<panel
			onmouseover="ShowWidget('{tooltip}');"
			onmouseout="HideWidget('{tooltip}');"
			tooltip="repick_hero_tip{index}"
			watch="HeroSelectPlayerInfo{index}"
			ontrigger="{panel_ontrigger}"
		>
			<button
				name="{button_name}"
				color="invisible"
				visible="{visible=true}"
				onmouseover="PlaySound('/shared/sounds/ui/button_over_02.wav'); ShowWidget('{tooltip}');"
				onmouseout="HideWidget('{tooltip}');"
				onclick="{onclick} RefreshCursor();"
				watch="HeroSelectPlayerInfo{index}"
				ontrigger="{button_ontrigger}"
			>
				<widgetstate statename="up">
					<image texture="/ui/elements/heroselection/repick_hero_up.tga" />
				</widgetstate>

				<widgetstate statename="over">
					<image texture="/ui/elements/heroselection/repick_hero_over.tga" />
				</widgetstate>
				
				<widgetstate statename="down" x="1" y="1">
					<image texture="/ui/elements/heroselection/repick_hero_down.tga" />
				</widgetstate>

				<widgetstate statename="disabled">
					<image texture="/ui/elements/heroselection/repick_hero_disabled.tga" />
				</widgetstate>
			</button>
		</panel>
	</template>
	

	<!-- Ready Button -->
	<template name="picker_ready_button">
		<panel color="invisible" noclick="1">
		
			<button
				color="invisible"
				visible="{visible=true}"
				onmouseover="ShowWidget('ready_tip{index}');" onmouseout="HideWidget('ready_tip{index}');"
				onclick="PlaySound('{sound=/shared/sounds/ui/ccpanel/button_click_02.wav}'); {onclick}"
				watch="{watch=}"
				ontrigger="{ontrigger=}"

			>
				<widgetstate statename="up">
					<image texture="/ui/elements/heroselection/ready_up.tga" />
					<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="{font=dyn_9}" color="white" shadow="true" shadowcolor="black" watch="{labelwatch}" ontrigger="{labelontrigger}">{label}</label>
				</widgetstate>
				
				<widgetstate statename="over">
					<image texture="/ui/elements/heroselection/ready_over.tga" />
					<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="{font=dyn_9}" color="white" shadow="true" shadowcolor="black" watch="{labelwatch}" ontrigger="{labelontrigger}">{label}</label>
				</widgetstate>
				
				<widgetstate statename="down" x="1" y="1">
					<image texture="/ui/elements/heroselection/ready_down.tga" />
					<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="{font=dyn_9}" color="yellow" shadow="true" shadowcolor="black" watch="{labelwatch}" ontrigger="{labelontrigger}">{label}</label>
				</widgetstate>
				
				<widgetstate statename="disabled">
					<image texture="/ui/elements/heroselection/ready_down.tga" rendermode="grayscale"/>
					<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="{font=dyn_9}" color="white" shadow="true" shadowcolor="black" watch="{labelwatch}" ontrigger="{labelontrigger}">{label}</label>
				</widgetstate>
			</button>
		
		</panel>
	</template>
	
	<!-- Player -->
	<template name="player_info">
		<panel width="18.0h" height="18.0h" align="center" color="invisible" noclick="1">
			
<!-- Me Indicator -->
			<panel
				name="yourslot{index}"
				noclick="1"
				x="-14.6h" y="0.4h"
				width="20h" height="10h"
				valign="center"
				align="center"
				color="0 0 1 1"
				visible="false"
				watch="HeroSelectPlayerInfo{index}"
				ontrigger="
					SetVisible(param1);
					SetColor(param4);
					If({index} ge 5, SetTexture('/ui/elements/me_right.tga'));
					If({index} ge 5, SetX('14.6h'));"
				texture="/ui/elements/me_left.tga"
			/>

                              <!-- Swap -->
			<panel>
				<button
					x="-25%"
					width="25%" height="25@"
					align="right" valign="center"
					texture="/ui/elements/heroselection/swap_hero.tga"
					watch="HeroSelectPlayerInfo{index}"
					onmouseover="ShowWidget('swap_hero_tip{index}');"
					onmouseout="HideWidget('swap_hero_tip{index}');"
					ontrigger="If(!param1 and param8 and !param9 and param11 and !param12, SlideX(0, 500), SlideX('-25%', 500));"
					onclick="SwapHeroRequest({index} % 5); PlaySound('/shared/sounds/ui/button_click_02.wav'); HideWidget('swap_hero_tip{index}');"
				/>
			</panel>

			<!-- Portrait -->
			<panel y="5.0h" width="9.0h" height="9.0h" align="center" color="black" />
			<panel y="5.0h" width="8.4h" height="8.4h" align="center">
				<image width="-1.0h" height="-1.0h" texture="/ui/elements/heroselection/nopick_{team}.tga" align="center" valign="center"/>
				<image
					watch="HeroSelectPlayerInfo{index}"
					ontrigger="
						SetVisible(param15);
						SetTexture(param6);
						SetOnmouseover('ShowWidget(\'hero_info\'); GetHeroInfo(\'' # param13 # '\');');"
					onmouseout="HideWidget('hero_info');"
				
				/>
				
				<!-- Lock -->
				<panel
					x="-0.9h" y="-0.8h"
					width="2.9h" height="2.9h"
					align="right" valign="bottom"
					texture="/ui/elements/lock.tga"
					watch="HeroSelectPlayerInfo{index}"
					ontrigger="SetVisible(param16);"
					noclick="true"
				/>

				<!-- Select effect -->
				<panel
					visible="0" y="-2%" valign="center" color="invisible" align="center"
					noclick="true" passivechildren="true"
					watch="HeroSelectPlayerInfo{index}"
					ontrigger="SetVisible(param15);"
				>
					<modelpanel	
						onshow="PlaySound('/shared/sounds/ui/hero_select/button_ready2.wav'); SetEffect('/ui/common/models/hero_select/hero_select.effect'));"
						model="common/models/hero_select/select.mdf"
						anim="idle"
						camerapos="0 1700 0"
						cameraangles="0 0 180"
						camerafov="2.0"
						cameranear="1"
						carmerafar="7000"
						depthclear="false"
						depthcompress="true"			
						sunazimuth="140"
						sunaltitude="10"
						suncolor="1.4 1.3 1"
					/>	
				</panel>

				<!-- Ready Effect -->
				<panel name="hero_ready_effect{index}"  visible="0" y="-2%"  valign="center"  color="invisible" align="center"
					noclick="true" passivechildren="true"
					watch="HeroSelectPlayerInfo{index}"
					ontrigger="SetVisible(param10);"
				>
					<modelpanel	
						watch="HeroSelectPlayerInfo{index}"
						ontrigger="SetEffect('/ui/common/models/hero_select/hero_ready.effect'));"
						model="common/models/hero_select/circle.mdf"
						anim="idle"
						camerapos="0 1700 0"
						cameraangles="0 0 180"
						camerafov="2.0"
						cameranear="1"
						carmerafar="7000"
						depthclear="false"
						depthcompress="true"			
						sunazimuth="140"
						sunaltitude="10"
						suncolor="1.4 1.3 1"
					/>	
				</panel>	

				<!-- Swap Effect -->
				<panel 
					visible="0" y="-2%" valign="center" color="invisible" align="center"
					noclick="true" passivechildren="true"
					watch="HeroSelectPlayerInfo{index}"
					ontrigger="SetVisible(param9 and param11);"
				>
					<modelpanel
						watch="HeroSelectPlayerInfo{index}"
						ontrigger="SetEffect('/ui/common/models/hero_select/hero_swap.effect'));"
						model="common/models/hero_select/select.mdf"
						anim="idle"
						camerapos="0 1700 0"
						cameraangles="0 0 180"
						camerafov="2.0"
						cameranear="1"
						carmerafar="7000"
						depthclear="false"
						depthcompress="true"			
						sunazimuth="140"
						sunaltitude="10"
						suncolor="1.4 1.3 1"
					/>	
				</panel>

				<image
					name="hero_loading_gloss"
					align="center" valign="center"
					noclick="true" passivechildren="true"
					texture="/ui/common/portrait_gloss_up.tga"
					rendermode="additive"
				/>
				
				<button
					color="invisible"
					watch="HeroSelectPlayerInfo{index}"
					ontrigger="SetVisible(!param1 and param9 and param11);"
					onmouseover="ShowWidget('swap_hero_accept_tip{index}');"
					onmouseout="HideWidget('swap_hero_accept_tip{index}');"
					onclick="SwapHeroRequest({index} % 5, 0, 0); HideWidget('swap_hero_accept_tip{index}'); PlaySound('/shared/sounds/ui/hero_select/button_swap.wav');"
				/>
			</panel>

			<!-- Ready -->
			<panel x="0.3h" y="2.95h" width="9.2h" height="2.3h" align="right" onmouseover="ShowWidget('ready_tip{index}');" onmouseout="HideWidget('ready_tip{index}');">
				<!-- Button -->
				<instance
					name="picker_ready_button"
					index="{index}"
					label="hselect_button_ready"
					onclick="Ready();"
					watch="HeroSelectPlayerInfo{index}"
					ontrigger="
						SetVisible(param1 and !param10);
						SetEnabled(param15);
						"
				/>
				
				<!-- Ready / Not Ready Status -->
				<panel noclick="1">
					<image
						noclick="1"
						watch="HeroSelectPlayerInfo{index}"
						ontrigger="
							SetVisible(!param1 or param10);
							If(param10,
								SetTexture('/ui/elements/heroselection/ready.tga'),
								SetTexture('/ui/elements/heroselection/not_ready.tga'));
							If(param10,
								PlaySound('/shared/sounds/ui/hero_select/button_ready.wav'));

							"
					/>
					<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="dyn_9" color="white" shadow="true" shadowcolor="black" noclick="1">hselect_button_ready</label>
				</panel>
			</panel>
			
			<image texture="/ui/elements/heroselection/hero_selection_frame.tga" noclick="1"/>
			
			<!-- Player Color -->
			<image 
				texture="/ui/elements/heroselection/hero_selection_color.tga"
				noclick="1"
				watch="HeroSelectPlayerInfo{index}"
				ontrigger="SetColor(param4);"
			/>
			
			<!-- Repick -->
			<panel y="3.0h" width="2.2h" height="2.2h" align="center" color="invisible" noclick="1">	
				<image width="140%" height="140%" align="center" valign="center" texture="/ui/elements/cc_config_circhl.tga"/>
				<instance
					name="player_pick_random"
					onclick="RepickHero(); HideWidget('repick_hero_tip{index}'); PlaySound('/shared/sounds/ui/hero_select/button_repick.wav');"
					tooltip=""
					index="{index}"
					panel_ontrigger="SetVisible(param1);"
					button_ontrigger="SetEnabled(param7 and param15);"
				/>
			</panel>			
			
			<!-- Unit Name -->
			<panel y="1.1h" height="2.0h" width="-5.0h" align="center" valign="top" color="invisible" noclick="1">
				<label height="100%" textvalign="center" textalign="center" font="dyn_10" color="white" shadow="1" watch="HeroSelectPlayerInfo{index}" ontrigger="SetText(param5);"/>
			</panel>
			
			<!-- Gold -->
			<panel x="1.4h" y="3.25h" height="1.7h" width="6.0h" align="left" valign="top" color="invisible" onmouseover="ShowWidget('gold_tip{index}');" onmouseout="HideWidget('gold_tip{index}');">
				<label height="100%" width="-1.6h" textvalign="center" textalign="center" font="dyn_9" color="white" shadow="1" watch="HeroSelectPlayerInfo{index}" ontrigger="SetText(param17);" noclick="1"/>
				<image x="-0.2h" width="1.6h" height="1.6h" valign="center" align="right" texture="/ui/common/gold.tga" noclick="1"/>
			</panel>
			
			<!-- Selection Indicator -->
			<panel
				name="arrow_panel{index}"
				noclick="1"
				x="-5.6h" y="0.4h"
				width="12h" height="7h"
				valign="center"
				align="center"
				color="invisible"
				visible="false"
				watch="HeroSelectPlayerInfo{index}"
				ontrigger="SetVisible(param14);"
			>
				<modelpanel
					watch="HeroSelectPlayerInfo{index}"
					ontrigger="if({index} ge 5, SetAnim('idle2')); if({index} ge 5, Call('arrow_panel{index}','SetX(\'5.4h\');'));"					
					name="arrow{index}"
					visible="1"
					noclick="true"
					model="common/models/arcade_text/arrow.mdf"
					anim="idle"
					camerapos="0 20 0"
					cameraangles="0 0 180"
					camerafov="45"
					cameranear="1"
					carmerafar="5000"
					depthclear="false"
					depthcompress="true"
					sunazimuth="90"
					sunaltitude="10"
					suncolor="3.2 2 2"						
				/>				
			</panel>
			<!-- Player Name -->
			<panel y="-2.5h" height="2.0h" width="-5.0h" align="center" valign="bottom" color="invisible" noclick="1">
				<label height="100%" textvalign="center" textalign="center" font="dyn_10" color="white" shadow="1" watch="HeroSelectPlayerInfo{index}" ontrigger="SetText(param3);"/>
			</panel>			
			
		</panel>	
	</template>
	
	<!-- Team Lists -->
	<template name="team_list1">
		<panel height="-4.0h" valign="top" color="invisible" noclick="1" float="bottom" reverse="1">
			<panel height="20%" color="invisible" noclick="1" watch="HeroSelectPlayerInfo0" ontrigger="SetVisible(param0 ge 0);" sticky="true" stickytoinvis="false">
				<instance name="player_info" index="0" team="legion" button_align="right"/>
			</panel>
			<panel height="20%" color="invisible" noclick="1" watch="HeroSelectPlayerInfo1" ontrigger="SetVisible(param0 ge 0);" sticky="true" stickytoinvis="false">
				<instance name="player_info" index="1" team="legion" button_align="right"/>
			</panel>
			<panel height="20%" color="invisible" noclick="1" watch="HeroSelectPlayerInfo2" ontrigger="SetVisible(param0 ge 0);" sticky="true" stickytoinvis="false">
				<instance name="player_info" index="2" team="legion" button_align="right"/>
			</panel>
			<panel height="20%" color="invisible" noclick="1" watch="HeroSelectPlayerInfo3" ontrigger="SetVisible(param0 ge 0);" sticky="true" stickytoinvis="false">
				<instance name="player_info" index="3" team="legion" button_align="right"/>
			</panel>
			<panel height="20%" color="invisible" noclick="1" watch="HeroSelectPlayerInfo4" ontrigger="SetVisible(param0 ge 0);" sticky="true" stickytoinvis="false">
				<instance name="player_info" index="4" team="legion" button_align="right"/>
			</panel>			
			<!-- Footer - Always Display -->
			<panel height="9.0h" noclick="1" sticky="true" stickytoinvis="false">
				<image y="-3.6h" x="-0.1h" width="36.0h" height="9.0h" align="left" valign="bottom" texture="/ui/elements/heroselection/hero_selection_footer_left.tga" noclick="1"/>
			</panel>
		</panel>
	</template>

	<template name="team_list2">
		<panel height="-4.0h" valign="top" color="invisible" noclick="1" float="bottom" sticky="true" reverse="1">			
			<panel height="20%" color="invisible" noclick="1" watch="HeroSelectPlayerInfo5" ontrigger="SetVisible(param0 ge 0);" sticky="true" stickytoinvis="false">
				<instance name="player_info" index="5" team="hellbourne" button_align="left"/>
			</panel>
			<panel height="20%" color="invisible" noclick="1" watch="HeroSelectPlayerInfo6" ontrigger="SetVisible(param0 ge 0);" sticky="true" stickytoinvis="false">
				<instance name="player_info" index="6" team="hellbourne" button_align="left"/>
			</panel>
			<panel height="20%" color="invisible" noclick="1" watch="HeroSelectPlayerInfo7" ontrigger="SetVisible(param0 ge 0);" sticky="true" stickytoinvis="false">
				<instance name="player_info" index="7" team="hellbourne" button_align="left"/>
			</panel>
			<panel height="20%" color="invisible" noclick="1" watch="HeroSelectPlayerInfo8" ontrigger="SetVisible(param0 ge 0);" sticky="true" stickytoinvis="false">
				<instance name="player_info" index="8" team="hellbourne" button_align="left"/>
			</panel>
			<panel height="20%" color="invisible" noclick="1" watch="HeroSelectPlayerInfo9" ontrigger="SetVisible(param0 ge 0);" sticky="true" stickytoinvis="false">
				<instance name="player_info" index="9" team="hellbourne" button_align="left"/>
			</panel>
			<!-- Footer - Always Display -->
			<panel height="9.0h" noclick="1" sticky="true" stickytoinvis="false">
				<image y="-3.6h" x="-2.0h" width="36.0h" height="9.0h" align="left" valign="bottom" texture="/ui/elements/heroselection/hero_selection_footer_right.tga" noclick="1"/>
			</panel>
		</panel>
	</template>

	<!-- Match Start Timer -->
	<template name="match_timer">
		<panel y="-2.0h" valign="top" width="14.0h" height="7.4h" align="center" texture="/ui/elements/heroselection/timer_base.tga">
			<label
				height="85%" width="100%"
				align="center"
				textvalign="center" textalign="center"
				color="white"
				font="dyn_12" shadow="1"
				watch="HeroSelectTimer"
				ontrigger="SetText(FtoT(param0, 1, 0, '-'));"
			/>
		</panel>
	</template>

	<!-- Random button -->
	<template name="random_button">
		<panel y="0" width="{width=16h}" height="{height=8h}" align="center" noclick="true">
			<panel width="45%" align="center" onmouseover="ShowWidget('random_hero_tip');" onmouseout="HideWidget('random_hero_tip');">
				<button
					color="invisible"
					onmouseover="PlaySound('/shared/sounds/ui/button_over_02.wav'); ShowWidget('random_hero_tip');"
					onmouseout="HideWidget('random_hero_tip');"
					onclick="PlaySound('/shared/sounds/ui/hero_select/button_random.wav'); RandomHero();"
					watch="HeroSelectInfo"
					ontrigger="SetEnabled(!param7);"
				>
					<widgetstate statename="up">
						<image width="222%" align="center" texture="/ui/elements/heroselection/random_up.tga" noclick="true"/>
					</widgetstate>

					<widgetstate statename="over">
						<image width="222%" align="center" texture="/ui/elements/heroselection/random_over.tga" noclick="true"/>
					</widgetstate>

					<widgetstate statename="down">
						<image width="222%" align="center" texture="/ui/elements/heroselection/random_down.tga" noclick="true"/>
					</widgetstate>

					<widgetstate statename="disabled">
						<image width="222%" align="center" texture="/ui/elements/heroselection/random_disabled.tga" noclick="true"/>
					</widgetstate>
				</button>
			</panel>
		</panel>
	</template>
	
	<!-- Hero Icon -->
	<template name="hero_icon">
		<panel
			width="{width}" height="{height}"
			color="white"
			visible="true"
			texture="/ui/elements/hero_picker_icon.tga"
			valign="{valign}" align="{align}"
			watch="HeroSelectHeroList{index}"
			ontrigger="
				If(param2 and param3, SetTexture(param1), SetTexture('/ui/elements/hero_picker_icon.tga'));
				If(param4 or !param3 or !param2, SetRenderMode('normal'), SetRenderMode('grayscale'));
				"
		>
			<button
				texture="/ui/elements/button_bevel.tga"
				watch="HeroSelectHeroList{index}"
				onclick="PlaySound('/shared/sounds/ui/error.wav');"
				ontrigger="
					If(param4,
						SetOnclick('SpawnHero(\'' # param0 # '\'); PlaySound(\'/shared/sounds/ui/hero_select/button_select.wav\');'),
						SetOnclick('PlaySound(\'/shared/sounds/ui/error.wav\');')
						);
					If(param2,
						SetOnmouseover('ShowWidget(\'hero_info\'); GetHeroInfo(\'' # param0 # '\');'),
						SetOnmouseover('')
						);
					"
				onmouseout="HideWidget('hero_info');"

			/>
		</panel>

	</template>

	<!-- Hero List - All Heroes -->
	<template name="hero_list_ap">
		<panel float="bottom" padding=".25h" color="invisible" noclick="1" visible="1" name="heroes">
			<!-- Top Row -->
			<panel height="10%" color="invisible">
				<image texture="/ui/elements/heroselection/picker_ap.tga" y="-0.4h" width="101%" align="center" />
				<image texture="/ui/elements/heroselection/team_legion.tga" x="18%" y="-0.4h" width="420@" height="105%" align="left" />
				<image texture="/ui/elements/heroselection/team_hellbourne.tga" x="-18%" y="-0.4h" width="420@" height="105%" align="right" />
			</panel>

			<!-- Row 1 -->
			<panel height="30%" color="invisible" name="agility">
				<frame texture="/ui/info/backdrop.tga" width="49%" borderthickness="5.0h" color="#0aa30a" />
				<frame texture="/ui/info/backdrop.tga" width="49%" align="right" borderthickness="5.0h" color="#ff3131" />
				<image x="-0.7%" width="33.75@" height="135%" texture="/ui/elements/heroselection/category_agi.tga"/>
				<image x="0.7%" width="33.75@" height="135%" align="right" texture="/ui/elements/heroselection/category_agi_2.tga"/>
				
				<!-- Legion -->
				<panel x="5.3%" width="44%" height="90%" valign="center" color="invisible" float="bottom">
					<panel height="32%" color="invisible" float="right">
						<instance name="hero_icon" width="100@" height="100%" index="0" />
						<instance name="hero_icon" width="100@" height="100%" index="1" />
						<instance name="hero_icon" width="100@" height="100%" index="2" />
						<instance name="hero_icon" width="100@" height="100%" index="3" />
						<instance name="hero_icon" width="100@" height="100%" index="4" />
					</panel>
					<panel height="32%" color="invisible" float="right">
						<instance name="hero_icon" width="100@" height="100%" index="5" />
						<instance name="hero_icon" width="100@" height="100%" index="6" />
						<instance name="hero_icon" width="100@" height="100%" index="7" />
						<instance name="hero_icon" width="100@" height="100%" index="8" />
						<instance name="hero_icon" width="100@" height="100%" index="9" />
					</panel>
