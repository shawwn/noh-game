			<panel y="0.4h" width="-2.0h" align="center" float="bottom" padding="0.2h" noclick="1">
				<panel height="2.2h" color="invisible" noclick="1" float="right">
					<panel width="36%" valign="center" color="invisible" noclick="1">
						<instance name="game_option" option="norepick" display_label="general_mode_no_hero_repick"/>
					</panel>
					<panel width="32%" valign="center" color="invisible" noclick="1">
						<instance name="game_option" option="noswap" display_label="general_mode_no_hero_swap"/>
					</panel>
					<panel width="31%" valign="center" color="invisible" noclick="1">
						<instance name="game_option" option="allowveto" display_label="general_mode_allow_veto"/>
					</panel>
				</panel>
				<panel height="2.2h" color="invisible" noclick="1" float="right">
					<panel width="36%" valign="center" color="invisible" noclick="1">
						<instance name="game_option" option="noagility" display_label="general_mode_no_agi_heroes"/>
					</panel>
					<panel width="32%" valign="center" color="invisible" noclick="1">
						<instance name="game_option" option="nointelligence" display_label="general_mode_no_intel_heroes"/>
					</panel>
					<panel width="31%" valign="center" color="invisible" noclick="1">
						<instance name="game_option" option="nostrength" display_label="general_mode_no_str_heroes"/>
					</panel>
				</panel>
				<panel height="2.2h" color="invisible" noclick="1" float="right">
					<panel width="36%" valign="center" color="invisible" noclick="1">
						<instance name="game_option" option="dropitems" display_label="general_mode_drop_items"/>
					</panel>
					<panel width="32%" valign="center" color="invisible" noclick="1">
						<instance name="game_option" option="nopowerups" display_label="general_mode_no_runes"/>
					</panel>
					<panel width="31%" valign="center" color="invisible" noclick="1">
						<instance name="game_option" option="norespawntimer" display_label="general_mode_no_respawn_timer"/>
					</panel>
				</panel>
				<panel height="2.2h" color="invisible" noclick="1" float="right">
					<panel width="36%" valign="center" color="invisible" noclick="1">
						<instance name="game_option" option="reverseselect" display_label="general_mode_reverse_hero_select"/>
					</panel>
					<panel width="32%" valign="center" color="invisible" noclick="1">
						<instance name="game_option" option="allowduplicate" display_label="general_mode_duplicate_heroes"/>
					</panel>
