						<instance name="game_option" option="alternatepicks" display_label="general_mode_alt_hero_pick"/>
					</panel>
				</panel>
			</panel>

			<!-- Dev Hero -->
			<panel x="-1" y="1" height="2.0h" width="3.9h" valign="top" align="right" color="1 1 0 .2" noclick="1" watch="Dev" ontrigger="SetVisible(param);">
				<instance name="game_option" option="devheroes" display_label="dh"/>
			</panel>

		</panel>
	</template>
	
	<template name="local_options">
		<instance name="game_options_advanced_empty" y="27.8h" />
		
		<panel name="{id}_options" color="invisible" noclick="1" visible="0">
			<instance name="game_options_advanced" y="27.8h" />
		</panel>
	</template>
	
	<template name="remote_options">
		<instance name="game_options_advanced_empty" y="27.8h" />
		
		<panel name="{id}_options" color="invisible" noclick="1" visible="0">
			<instance name="game_options_advanced" y="27.8h" />
		</panel>
	</template>
	
	<!-- Game Setup Options -->
	<template name="game_settings_panel">
		<panel onload="CreateString('_game_options');" y="{y=3.5h}" color="invisible" noclick="1">
		
			<panel y="{button_y=38.6h}" width="-2.0h" align="center" height="2.5h" noclick="1" visible="1" color="invisible">
			
				<panel width="6.4h" color="invisible" noclick="1" visible="{customstatspost_visible=0}">
					<button
						color="imvisible"
						onclick="Call('custom_url_post', 'FadeIn(250);');"
						onmouseover="ShowWidget('custom_post_tip');"
						onmouseout="HideWidget('custom_post_tip');"
					>
						<widgetstate statename="up">
							<frame texture="/ui/info/button.tga" borderthickness="0.8h" color="1 1 1 1"/>				
							<image width="108%" height="27@" align="center" valign="center" texture="/ui/elements/custom_post.tga" noclick="1"/>
						</widgetstate>					
						<widgetstate statename="over">
							<frame texture="/ui/info/button_over.tga" borderthickness="0.8h" />				
							<image width="108%" height="27@" align="center" valign="center" texture="/ui/elements/custom_post.tga" noclick="1"/>
						</widgetstate>						
						<widgetstate statename="down" x="1" y="1">
							<frame texture="/ui/info/button_down.tga" borderthickness="0.8h" color="white"/>					
							<image width="108%" height="27@" align="center" valign="center" texture="/ui/elements/custom_post.tga" noclick="1"/>
						</widgetstate>
					</button>					
				</panel>
								
				<panel x="10.0h" color="invisible" noclick="1" visible="{private_visible=0}">
					<instance name="game_option" option="private" display_label="general_private_game" />
				</panel>
						
				<instance
					name="button2"
					align="right"
					x="-13.0h"
					id="game_options_cancel_button_{id}"
					width="8.6h"
					font="dyn_10"
					label="general_cancel"
					borderthickness=".8h"
					onclick="PlaySound('/shared/sounds/ui/slide2.wav'); {cancel_command}"
					onmouseover="PlaySound('/shared/sounds/ui/button_over_01.wav');"
				/>
				
				<instance
					name="button2"
					id="game_options_local_start_button2_{id}"
					align="right" width="12.0h" 
					font="dyn_10"
					label="general_create_game"
					borderthickness=".8h"
					onclick="
						_game_options =
							'map:' # _game_map_name # ' ' #
							'mode:' # _game_mode # ' ' #
							'teamsize:' # _game_num_players # ' ' #
							'spectators:' # _game_num_spectators # ' ' #
							'referees:' # _game_num_spectators # ' ' #
							'tier:' # _game_tier # ' ' #
							_game_option_private # ' ' #
							_game_option_noleaver # ' ' #
							_game_option_nostats # ' ' #
							_game_option_allheroes # ' ' #
							_game_option_forcerandom # ' ' #
							_game_option_alternatepicks # ' ' #
							_game_option_norepick # ' ' #
							_game_option_noswap # ' ' #
							_game_option_noagility # ' ' #
							_game_option_nointelligence # ' ' #
							_game_option_nostrength # ' ' #
							_game_option_norespawntimer # ' ' #
							_game_option_dropitems # ' ' #
							_game_option_nopowerups # ' ' #
							_game_option_easymode # ' ' #
							_game_option_allowduplicate # ' ' #
							_game_option_reverseselect # ' ' #
							_game_option_notop # ' ' #
							_game_option_nomiddle # ' ' #
							_game_option_nobottom # ' ' #
							_game_option_allowveto # ' ' #
							_game_option_shuffleteams # ' ' #
							_game_option_autobalance # ' ' #
							_game_option_devheroes # ' ' #
							_game_option_cheats;
						StartGame(_game_server_type, gamename_textbox_{id}, _game_options);
						"
					onmouseover="PlaySound('/shared/sounds/ui/button_over_01.wav');"
					watch="ConnectionStatus"
					ontrigger="SetVisible(!param0);"
				/>
				
				<instance
					name="button2"
					id="game_options_connected_start_button2_{id}"
					align="right" width="12.0h" 
					font="dyn_10"
					label="general_create_game"
					borderthickness=".8h"
					onclick="
						_game_options =
							'map:' # _game_map_name # ' ' #
							'mode:' # _game_mode # ' ' #
							'teamsize:' # _game_num_players # ' ' #
							'spectators:' # _game_num_spectators # ' ' #
							'referees:' # _game_num_spectators # ' ' #
							'tier:' # _game_tier # ' ' #
							_game_option_private # ' ' #
							_game_option_noleaver # ' ' #
							_game_option_allheroes # ' ' #
							_game_option_forcerandom # ' ' #
							_game_option_alternatepicks # ' ' #
							_game_option_norepick # ' ' #
							_game_option_noswap # ' ' #
							_game_option_noagility # ' ' #
							_game_option_nointelligence # ' ' #
							_game_option_nostrength # ' ' #
							_game_option_norespawntimer # ' ' #
							_game_option_dropitems # ' ' #
							_game_option_nopowerups # ' ' #
							_game_option_easymode # ' ' #
							_game_option_allowduplicate # ' ' #
							_game_option_reverseselect # ' ' #
							_game_option_notop # ' ' #
							_game_option_nomiddle # ' ' #
							_game_option_nobottom # ' ' #
							_game_option_allowveto # ' ' #
							_game_option_shuffleteams # ' ' #
							_game_option_devheroes # ' ' #
							_game_option_cheats;
						SendCreateGameRequest(gamename_textbox_{id}, _game_options);
						"
					onmouseover="PlaySound('/shared/sounds/ui/button_over_01.wav');"
					watch="ConnectionStatus"
					ontrigger="SetVisible(param0);"
				/>
			</panel>
			
