	<template name="server_combobox_local">
		<instance
			name="Noptions_combobox"
			id="select_local_server_idle_{id}"
			align="left"
			x="0"
			width="-1.8h"
			itemwidth="100%"
			maxlistheight="3"
			listbgheight="8.7h"
			cvar=""
			data=""
			populate="
				AddTemplateListItem('combobox_item_icon', 'local', 'label', 'game_setup_local', 'texture', '/ui/icons/local.tga');
				AddTemplateListItem('combobox_item_icon', 'local_automatic', 'label', 'game_setup_automatic', 'texture', '/ui/icons/automatic.tga');
				AddTemplateListItem('Ncombobox_item', 'browse', 'label', 'game_setup_browse', 'onselect', 'Call(\'local_server_browser\', \'FadeIn(250);\');', 'command', 'true');"
			onload="CreateString('_game_server_type', ''); CreateString('_game_local_server_type', 'local'); SetSelectedItemByValue(_game_local_server_type, false);"
			onselect="_game_local_server_type = this; _game_server_type = this;"
			watch="ConnectionStatus"
			ontrigger="SetVisible(!param0);"
			onshow="_game_server_type = _game_local_server_type;"
		/>
		
		<instance
			name="Noptions_combobox"
			id="select_local_server_connected_{id}"
			align="left"
			x="0"
			width="-1.8h"
			itemwidth="100%"
			maxlistheight="4"
			listbgheight="10.7h"
			cvar=""
			data=""
			populate=""
			onselect="Call('select_local_server_idle_{id=unset}', 'SetSelectedItemByValue(\'' # this # '\', false);');"
			watch="ConnectionStatus"
			ontrigger="
				Clear();
				AddTemplateListItem('Ncombobox_item', 'selected', 'label', param1);
				AddTemplateListItem('combobox_item_icon', 'local', 'label', 'game_setup_local', 'texture', '/ui/icons/local.tga', 'onselect', 'Disconnect();');
				AddTemplateListItem('combobox_item_icon', 'local_automatic', 'label', 'game_setup_automatic', 'texture', '/ui/icons/automatic.tga', 'onselect', 'Disconnect();');
				AddTemplateListItem('Ncombobox_item', 'browse', 'label', 'game_setup_browse', 'onselect', 'ShowWidget(\'local_server_browser\');', 'command', 'true');
				SetSelectedItemByValue('selected', false);
				SetVisible(param0);"
			visible="false"
		/>
	</template>
	
	<template name="server_combobox_main">
		<instance
			name="Noptions_combobox"
			id="select_server_idle_{id}"
			align="left"
			x="0"
			width="-1.8h"
			itemwidth="100%"
			maxlistheight="2"
			listbgheight="6.7h"
			cvar=""
			data=""
			populate="
				AddTemplateListItem('combobox_item_icon', 'remote_automatic', 'label', 'game_setup_automatic', 'texture', '/ui/icons/automatic.tga');
				AddTemplateListItem('Ncombobox_item', 'browse', 'label', 'game_setup_browse', 'onselect', 'Call(\'server_browser\', \'FadeIn(500);\');', 'command', 'true');"
			onload="CreateString('_game_server_type', ''); CreateString('_game_remote_server_type', 'remote_automatic'); SetSelectedItemByValue(_game_remote_server_type, false);"
			onselect="_game_remote_server_type = this; _game_server_type = this;"
			watch="ConnectionStatus"
			ontrigger="SetVisible(!param0);"
			onshow="_game_server_type = _game_remote_server_type;"
		/>
		
		<instance
			name="Noptions_combobox"
			id="select_server_connected_{id}"
			align="left"
			x="0"
			width="-1.8h"
			itemwidth="100%"
			maxlistheight="3"
			listbgheight="8.7h"
			cvar=""
			data=""
			populate=""
			onselect="Call('select_server_idle_{id}', 'SetSelectedItemByValue(\'' # this # '\', false);');"
			watch="ConnectionStatus"
			ontrigger="
				Clear();
				AddTemplateListItem('Ncombobox_item', 'selected', 'label', param1);
				AddTemplateListItem('combobox_item_icon', 'remote_automatic', 'label', 'game_setup_automatic', 'texture', '/ui/icons/automatic.tga', 'onselect', 'Disconnect();');
				AddTemplateListItem('Ncombobox_item', 'browse', 'label', 'game_setup_browse', 'onselect', 'ShowWidget(\'server_browser\');', 'command', 'true');
				SetSelectedItemByValue('selected', false);
				SetVisible(param0);"
			visible="false"
		/>
	</template>
	
	<!-- Additional Options Empty -->
	<template name="game_options_advanced_empty">
		<panel name="game_options_advanced_empty" y="{y=0.5h}" width="-2%" align="center" height="10.2h" color="0 0 0 1" visible="1">
			<frame texture="/ui/info/backdrop.tga" borderthickness="4.0h" color="1 1 1 .5" noclick="1" />
			<frame color=".05 .07 .1 .5" bordercolor="#455166" borderthickness="1" noclick="1" />		
			<image texture="/ui/elements/logo.tga" width="100@" height="100%" align="center" valign="center" color="1 1 1 .3" noclick="1" />
			
			<panel y="-0.4h" height="2.4h" width="50%" valign="center" align="center" color="invisible">
				<instance name="more_options" display_label="game_setup_advanced_options" id="{id}"/>
			</panel>
					
		</panel>	
	</template>
		
