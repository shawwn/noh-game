	<!-- Templates -->
	<template name="arcade_text">
		<modelpanel
			name="{msg}"
			visible="0"
			noclick="true"
			model="common/models/{msg}.mdf"
			anim="idle"
			cameradistance="0"
			lookat="false"
			camerapos="0 1300 0"
			cameraangles="0 0 180"
			camerafov="50"
			cameranear="1"
			carmerafar="5000"
			depthclear="false"
			depthcompress="true"
			
			sunazimuth="-90"
			sunaltitude="180"
			suncolor="{suncolor}"
			
			watch="{watch}"
			ontrigger="
				If({condition=true},
					Split(
						SetVisible(true),
						SetAnim('idle'),
						SetEffect('/ui/common/models/{set}/bloodlust.effect'),
						SleepWidget({lifetime}, 'SetVisible(false);')
					)
				);
			"
		/>
	</template>
	
	<template name="voice_set">
		<instance name="arcade_text" set="{set}" msg="{set}/denied" lifetime="{lifetime}" suncolor="{suncolor}" watch="EventTowerDeny" condition="StringEquals(param1, '{set}')"/>
		<instance name="arcade_text" set="{set}" msg="{set}/bloodlust" lifetime="{lifetime}" suncolor="{suncolor}" watch="EventFirstKill" condition="StringEquals(param1, '{set}')"/>
		<instance name="arcade_text" set="{set}" msg="{set}/hattrick" lifetime="{lifetime}" suncolor="{suncolor}" watch="EventMultiKill" condition="param == 1 and StringEquals(param1, '{set}')"/>
		<instance name="arcade_text" set="{set}" msg="{set}/quadkill" lifetime="{lifetime}" suncolor="{suncolor}" watch="EventMultiKill" condition="param == 2 and StringEquals(param1, '{set}')"/>
		<instance name="arcade_text" set="{set}" msg="{set}/annihilation" lifetime="{lifetime}" suncolor="{suncolor}" watch="EventMultiKill" condition="param == 3 and StringEquals(param1, '{set}')"/>
		<instance name="arcade_text" set="{set}" msg="{set}/bloodbath" lifetime="{lifetime}" suncolor="{suncolor}" watch="EventKillStreak" condition="param == 10 and StringEquals(param1, '{set}')"/>
		<instance name="arcade_text" set="{set}" msg="{set}/immortal" lifetime="{lifetime}" suncolor="{suncolor}" watch="EventKillStreak" condition="param == 15 and StringEquals(param1, '{set}')"/>
		<instance name="arcade_text" set="{set}" msg="{set}/genocide" lifetime="{lifetime}" suncolor="{suncolor}" watch="EventTeamWipe" condition="StringEquals(param1, '{set}')"/>
		<instance name="arcade_text" set="{set}" msg="{set}/smackdown" lifetime="{lifetime}" suncolor="{suncolor}" watch="EventSmackdown" condition="StringEquals(param1, '{set}')"/>
		<instance name="arcade_text" set="{set}" msg="{set}/humiliation" lifetime="{lifetime}" suncolor="{suncolor}" watch="EventHumiliation"  condition="StringEquals(param1, '{set}')"/>
		<instance name="arcade_text" set="{set}" msg="{set}/nemesis" lifetime="{lifetime}" suncolor="{suncolor}" watch="EventRival" condition="StringEquals(param1, '{set}')"/>
		<instance name="arcade_text" set="{set}" msg="{set}/payback" lifetime="{lifetime}" suncolor="{suncolor}" watch="EventPayback" condition="StringEquals(param1, '{set}')"/>
		<instance name="arcade_text" set="{set}" msg="{set}/ragequit" lifetime="{lifetime}" suncolor="{suncolor}" watch="EventRageQuit" condition="StringEquals(param1, '{set}')"/>
		<instance name="arcade_text" set="{set}" msg="{set}/victory" lifetime="{lifetime}" suncolor="{suncolor}" watch="EventVictory" condition="StringEquals(param1, '{set}')"/>
		<instance name="arcade_text" set="{set}" msg="{set}/defeat" lifetime="{lifetime}" suncolor="{suncolor}" watch="EventDefeat" condition="StringEquals(param1, '{set}')"/>
	</template>
	

	
	<template name="multi_select_unit">
		<panel width="4.0h" watch="SelectedVisible{index}" ontrigger="SetVisible(param);">
			<frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="3" watch="SelectedColor{index}" ontrigger="SetBorderColor(param);"/>
			<frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="3" color="0 0 0 .9"/>
			<frame texture="/ui/frames/roundframe.tga" watch="SelectedColor{index}" ontrigger="SetBorderColor(param);" borderthickness="3"/>

			<panel align="center" valign="center" width="85%" height="85%" float="bottom" padding="1%" color="invisible">
				<panel align="center" height="85@" width="85%" color="invisible" >
					<image
						watch="SelectedIcon{index}"
						ontrigger="SetTexture(param);"
						watch0="SelectedIllusion{index}"
						ontrigger0="If(param, SetRenderMode('grayscale'), SetRenderMode('normal')); If(param, SetColor('0.35 1.3 0.35'), SetColor('white'));"
					/>
				</panel>
				<panel height="7%" width="85%" color="black" watch="SelectedHealth{index}" ontrigger="SetVisible(param1 gt 0);">
					<panel color="red" watch="SelectedHealth{index}" ontrigger="SetWidth(FtoP(param2));"/>
					<panel color="yellow" watch="SelectedHealth{index}" ontrigger="SetWidth(FtoP(param2)); SetColor(1, 1, 0, Saturate((param2 * 2.5) - 0.25));" />
					<panel color="lime" watch="SelectedHealth{index}" ontrigger="SetWidth(FtoP(param2)); SetColor(0, 1, 0, Saturate((param2 * 2) - 1));" />
				</panel>
				<panel height="7%" width="85%" color="black" watch="SelectedMana{index}" ontrigger="SetVisible(param1 gt 0);">
					<panel color="blue" watch="SelectedMana{index}"  ontrigger="SetWidth(FtoP(param2));"/>
				</panel>
			</panel>
			
			<image
				texture="/ui/info/drop.tga"
				rendermode="additive"
				color="white"
				watch="SelectedActive{index}"
				ontrigger="SetVisible(param);"
			/>
			
			<panel
				color="invisible"
				onclick="SetControlUnit({index});"
				onrightclick="DeselectUnit({index});"
				onmouseover="ShowWidget('unit_tooltip_info{index}');"
				onmouseout="HideWidget('unit_tooltip_info{index}');"
			/>
		</panel>
	</template>


	
	

	<template name="ability_button">
		<panel>
			<panel
				watch="SelectedInventoryExists{slot}"
				ontrigger="SetVisible(!param);"
			>
				<image texture="/ui/legion/ability_coverup.tga"/>
			</panel>

			<panel
				watch="SelectedInventoryExists{slot}"
				ontrigger="SetVisible(param);"
				onclick="PrimaryAction({slot});"
				onrightclick="SecondaryAction({slot});"
				onmouseover="ShowWidget('ability_tooltip{slot}');"
				onmouseout="HideWidget('ability_tooltip{slot}');"
				watch2="EventSelectedInventoryUpgrade{slot}"
				ontrigger2="If(IsVisible(), Call('effect_panel', 'StartEffect(\'/ui/common/models/choose/choose.effect\', ' # GetAbsoluteFractionX(0.5) / GetScreenWidth() # ', ' # GetAbsoluteFractionY(0.5) / GetScreenHeight() # ', \'1 1 1 1\');'));"
			>
				<!-- Icon -->
				<image
					watch="SelectedInventoryIcon{slot}"
					ontrigger="SetTexture(param);"
					watch0="SelectedInventoryStatus{slot}"
					ontrigger0="If(param5, SetRenderMode('normal'), SetRenderMode('grayscale'));"
					noclick="true"
				/>

				<!-- Cooldown -->
				<piegraph start="270" end="-90" square="true" color="#000000cc" watch="SelectedInventoryCooldown{slot}" ontrigger="SetVisible(param1 > 0); SetValue(param2);" noclick="true"/>
				
				<!-- Timer -->
				<label
					y="0.2h"
					width="100%"
					height="100%"
					textvalign="top"
					textalign="center"
					color="white"
					shadow="true"
					font="dyn_12"
					watch="SelectedInventoryHasTimer{slot}"
					ontrigger="SetVisible(param);"
					watch1="SelectedInventoryTimer{slot}"
					ontrigger1="SetText(FtoT(param + 949));"
					noclick="true"
				/>

				<!-- Tooltip -->
				<panel
					onmouseover="ShowWidget('ability_tooltip{slot}');"
					onmouseout="HideWidget('ability_tooltip{slot}');"
					watch="SelectedInventoryStatus{slot}"
					ontrigger="SetVisible(param3 > 0 or param4 == 0); If(param3 > 0 or param4 == 0, SetColor('white'), SetColor('invisible'));"
				/>

				<!-- Highlight -->
				<panel
					texture="/ui/common/button_gloss_up.tga"
					rendermode="additive"
					onmouseover="ShowWidget('ability_tooltip{slot}');"
					onmouseout="HideWidget('ability_tooltip{slot}');"
					watch="SelectedInventoryCanActivate{slot}"
					ontrigger="SetVisible(param);"
					watch1="SelectedInventoryStatus{slot}"
					ontrigger1="SetEnabled(param3 > 0 or param4 == 0); If(param3 > 0 or param4 == 0, SetColor('#cccccc'), SetColor('invisible'));"
					interactive="false"
				/>

				<!-- Color overlay -->
				<image
					texture="$white"
					rendermode="overlay"
					watch="SelectedInventoryStatus{slot}"
					ontrigger="
						SetVisible(true);
						If(param3 == 0 and param4 > 0,
							SetColor('#404040'),
							If(param1,
								SetColor('#676045'),
								If(param2,
									SetColor('#4444ff'),
									SetVisible(false)
								)
							)
						);
						"
					noclick="true"
				/>

				<!--  silenced intentory section... can move or add a timer later -->
				<image align="center" valign="center" width="90%" height="90%" texture="elements/silence.tga" watch="Silenced" ontrigger="SetVisible(param);" color="1 1 1 1" noclick="1" visible="0"/>

				<!-- Charges -->
				<panel
					width="38.5%"
					height="38.5%"
					align="right"
					valign="bottom"
					watch="SelectedInventoryCharges{slot}"
					ontrigger="SetVisible(param gt 0);"
					noclick="true"
					passivechildren="true"
					color="invisible"
				>
					<frame color="0 0 0 .7" bordercolor="white" borderthickness="1"/>
					<label width="100%" height="100%" textalign="center" textvalign="center" color="white" shadow="true" font="dyn_8" watch="SelectedInventoryCharges{slot}" ontrigger="SetText(param);"/>
				</panel>

				<!-- Refresh effect -->
				<modelpanel
					visible="0"
					noclick="true"
					model="common/models/refresh/refresh.mdf"
					anim="idle"
					effect="common/models/refresh/refresh.effect"
					camerapos="0 1 0"
					cameraangles="0 0 180"
					camerafov="45"
					cameranear="1"
					carmerafar="100"
					depthclear="false"
					depthcompress="true"
					watch="EventSelectedInventoryReady{slot}"
					ontrigger="SetVisible(true); SetAnim('idle'); SetEffect('/ui/common/models/refresh/refresh.effect'); SleepWidget(1333, 'SetVisible(false);');"
					onshow="PlaySound('/shared/sounds/ui/ability_refresh.wav'); "
				/>

				<!-- Toggle active -->
				<frame
					borderthickness="2"
					bordercolor="orange"
					color="invisible"
					visible="false"
					watch="SelectedInventoryStatus{slot}"
					ontrigger="SetVisible(param0);"
					noclick="true"
					passivechildren="true"
				>
					<!--<image x="2" y="2" width="-4" height="-4" color="orange" texture="/ui/fps/selected.tga" rendermode="additive"/>-->
					<modelpanel
						visible="1"
						noclick="true"
						model="common/models/toggle/toggle.mdf"
						anim="idle"
						effect="common/models/toggle/toggle.effect"
						camerapos="0 1 0"
						cameraangles="0 0 180"
						camerafov="45"
						cameranear="1"
						carmerafar="100"
						depthclear="false"
						depthcompress="true"
						onshow="SetAnim('idle'); SetEffect('/ui/common/models/toggle/toggle.effect');"
					/>
				</frame>

				<!-- Activating -->
				<frame
					borderthickness="2"
					bordercolor="yellow"
					color="invisible"
					visible="false"
					watch="SelectedInventoryStatus{slot}"
					ontrigger="SetVisible(param6);"
					noclick="true"
					passivechildren="true"
				>
					<image x="2" y="2" width="-4" height="-4" color="yellow" texture="/ui/fps/selected.tga" rendermode="additive"/>
				</frame>
				
				<!-- Numeric cooldown timer -->
				<panel color="invisible" noclick="1">
					<label noclick="1" valign="center" align="center" textvalign="center" textalign="center" color="#87CEEB" shadow="1" font="dyn_10" watch="SelectedInventoryCooldown{slot}" ontrigger="SetVisible(param0); SetText(FtoT(param0, 0, 0, '-') # 's');" />
				</panel>
			</panel>
		</panel>
	</template>
	
