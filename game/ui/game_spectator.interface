						carmerafar="100"
						depthclear="false"
						depthcompress="true"
						onshow="SetAnim('idle'); SetEffect('/ui/common/models/toggle/toggle.effect');"
					/>
				</frame>

				<!-- Activating -->
				<frame
					borderthickness="2"
					bordercolor="yellow"
