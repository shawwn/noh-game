<?xml version="1.0" encoding="UTF-8"?>
<interface  onload="CreateInt('_lobbyCustomOpen', 0); CreateInt('_lobbyCreateOpen', 0); CreateInt('_lobbyQPOptionsOpen', 0); CreateInt('_lobbyQPOpen', 0);  CreateInt('_lobbyRankedOpen', 0); "  name="main_lobby" onshow="ShowWidget('main_lobby'); ChatConnect(); CheckReconnect();">
    <include file="/ui/ui_items.package"/>
    <include file="game_setup.package"/>
    
    <!-- Dynamic effects -->
    <effect file="/ui/common/effects/lobby_wide.effect" />
    <effect file="/ui/common/effects/lobby_thin.effect" />
    
    <!-- Templates -->
    <template name="filter_option">
        <button
            height="1.5h"
            valign="center"
            onload="CreateString('_filter_option_{option}', '{option}:false');"
            numstates="2"
            color="invisible"
            onclick="If(this, Set(_filter_option_{option}, '{option}:true'), Set(_filter_option_{option},'{option}:false')); RefreshWidget('filter_controller');"
        >
            <widgetstate statename="up1" float="right" padding="3%">
                <panel width="1.5h" color="invisible">
                    <frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="4" color="white"/>
                    <frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="6"/>
                </panel>
                <label width="-1.5h" height="100%" textvalign="center" font="dyn_9" color="white" shadow="true">{display_label}</label>
            </widgetstate>
            <widgetstate statename="over1" float="right" padding="3%">
                <panel width="1.5h" color="invisible">
                    <frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="4" color="white"/>
                    <frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="6"/>
                </panel>
                <label width="-1.5h" height="100%" textvalign="center" font="dyn_9" color="#3abde7" shadow="true">{display_label}</label>
            </widgetstate>
            <widgetstate statename="down1" float="right" padding="3%">
                <panel width="1.5h" color="invisible">
                    <frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="4" color="white"/>
                    <frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="6"/>
                </panel>
                <label width="-1.5h" height="100%" textvalign="center" font="dyn_9" color="#3abde7" shadow="true">{display_label}</label>
            </widgetstate>
            <widgetstate statename="up2" float="right" padding="3%">
                <panel width="1.5h" color="invisible">
                    <frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="4" color="white"/>
                    <frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="6"/>
                    <image x="0" y="0" width="75%" height="75%" align="center" valign="center" texture="/ui/elements/check.tga"/>
                </panel>
                <label width="-1.5h" height="100%" textvalign="center" font="dyn_9" color="#ffaa00" shadow="true">{display_label}</label>
            </widgetstate>
            <widgetstate statename="over2" float="right" padding="3%">
                <panel width="1.5h" color="invisible">
                    <frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="4" color="white"/>
                    <frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="6"/>
                    <image x="0" y="0" width="75%" height="75%" align="center" valign="center" texture="/ui/elements/check.tga"/>
                </panel>
                <label width="-1.5h" height="100%" textvalign="center" font="dyn_9" color="#3abde7" shadow="true">{display_label}</label>
            </widgetstate>
            <widgetstate statename="down2" float="right" padding="3%">
                <panel width="1.5h" color="invisible">
                    <frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="4" color="white"/>
                    <frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="6"/>
                    <image x="0" y="0" width="75%" height="75%" align="center" valign="center" texture="/ui/elements/check.tga"/>
                </panel>
                <label width="-1.5h" height="100%" textvalign="center" font="dyn_9" color="#3abde7" shadow="true">{display_label}</label>
            </widgetstate>
        </button>
    </template>
    
