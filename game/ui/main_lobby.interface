            <widgetstate statename="up1" float="right" padding="0.3h">
                <image width="100@" valign="center" texture="/ui/icons/{icon}"/>
                <panel width="1.5h" height="1.5h" valign="center" color="invisible">
                    <frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="4" color="white"/>
                    <frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="6"/>
                </panel>
                <label width="-1.5h" valign="center" height="100%" textvalign="center" font="{font=dyn_9}" color="white" shadow="true">{display_label}</label>
            </widgetstate>
            <widgetstate statename="over1" float="right" padding="0.3h">
                <image width="100@" valign="center" texture="/ui/icons/{icon}"/>
                <panel width="1.5h" height="1.5h" valign="center" color="invisible">
                    <frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="4" color="white"/>
                    <frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="6"/>
                </panel>
                <label width="-1.5h" valign="center" height="100%" textvalign="center" font="{font=dyn_9}" color="white" shadow="true">{display_label}</label>
            </widgetstate>
            <widgetstate statename="down1" float="right" padding="0.3h">
                <image width="100@" valign="center" texture="/ui/icons/{icon}"/>
                <panel width="1.5h" height="1.5h" valign="center" color="invisible">
                    <frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="4" color="white"/>
                    <frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="6"/>
                </panel>
                <label width="-1.5h" valign="center" height="100%" textvalign="center" font="{font=dyn_9}" color="white" shadow="true">{display_label}</label>
            </widgetstate>
            <widgetstate statename="up2" float="right" padding="0.3h">
                <image width="100@" valign="center" texture="/ui/icons/{icon}"/>
                <panel width="1.5h" height="1.5h" valign="center" color="invisible">
                    <frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="4" color="white"/>
                    <frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="6"/>
                    <image x="0.15h" y="0.15h" width="-0.3h" height="-0.3h" texture="/ui/elements/check.tga"/>
                </panel>
                <label width="-1.5h" valign="center" height="100%" textvalign="center" font="{font=dyn_9}" color="white" shadow="true">{display_label}</label>
            </widgetstate>
            <widgetstate statename="over2" float="right" padding="0.3h">
                <image width="100@" valign="center" texture="/ui/icons/{icon}"/>
                <panel width="1.5h" height="1.5h" valign="center" color="invisible">
                    <frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="4" color="white"/>
                    <frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="6"/>
                    <image x="0.15h" y="0.15h" width="-0.3h" height="-0.3h" texture="/ui/elements/check.tga"/>
                </panel>
                <label width="-1.5h" valign="center" height="100%" textvalign="center" font="{font=dyn_9}" color="white" shadow="true">{display_label}</label>
            </widgetstate>
            <widgetstate statename="down2" float="right" padding="0.3h">
                <image width="100@" valign="center" texture="/ui/icons/{icon}"/>
                <panel width="1.5h" height="1.5h" valign="center" color="invisible">
                    <frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="4" color="white"/>
                    <frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="6"/>
                    <image x="0.15h" y="0.15h" width="-0.3h" height="-0.3h" texture="/ui/elements/check.tga"/>
                </panel>
                <label width="-1.5h" valign="center" height="100%" textvalign="center" font="{font=dyn_9}" color="white" shadow="true">{display_label}</label>
            </widgetstate>
        </button>
    </template>
    
    <!-- MOTD -->
    <template name="motd">
        <panel noclick="1">
            <label width="100%" height="100%" textvalign="left" textalign="left" align="center" valign="center" font="dyn_10" color="white" shadow="true" wrap="true">MOTD</label>
        </panel>
    </template> 

    <!-- Lobby -->
    <template name="chat_list_entry">
        <panel name="entry_main_{name}" texture="/ui/elements/line_break.tga" color="1 1 1 .1" align="left" valign="center" float="right" padding="10@" passivechildren="true">
            <panel x="10@" width="75@" height="75%" sticky="true" stickytoinvis="false" onshow="If({admin=0} == 0, SetVisible(false));">
                <image rendermode="grayscale" texture="/ui/icons/op.tga" noclick="1" onshow="If({admin=0} != 1, SetVisible(false));"/> 
                <image texture="/ui/icons/op.tga" onshow="If({admin=0} != 2, SetVisible(false));"/> 
                <image texture="/ui/icons/s2.tga" onshow="If({admin=0} != 3, SetVisible(false));"/> 
            </panel>
            <panel width="75@" height="75%" sticky="true" stickytoinvis="false" onshow="If({ingame=0} == 0, SetVisible(false));">
                <image texture="/ui/icons/ingame_2.tga"/> 
            </panel>
            <panel sticky="true" stickytoinvis="false" stickysizing="true">
                <label y="-0.1h" textalign="left" textvalign="center" font="dyn_12" color="white" shadow="true" content="{name}" onshow="If({ingame=0}, SetColor('#888888'));"/>
            </panel>
        </panel>
    </template>
    
    <template name="channel_header">
        <panel name="main_lobby_channel_header_panel_{channel}" width="-1.0h" height="2.5h" align="center" watch="ChatLeftChannel" ontrigger="If(!{permanent} and (StringEmpty(param) or StringEquals(param, '{channel}')), DestroyWidget(), Split(ShowOnly('main_lobby_chat_panel_{channel}'), ShowOnly('channel_header_selected_{channel}')));">

            <frame name="channel_header_unselected_{channel}"
                group="channel_header_unselected"
                visible="1"
                texture="/ui/info/button.tga"
                borderthickness="0.3h"
                color="white"
            />

            <frame name="channel_header_selected_{channel}"
                group="channel_header_selected"
                visible="false"
                texture="/ui/info/button_down.tga"
                borderthickness="0.3h"
                color="white"
                onload="If({startselected}, ShowOnly('channel_header_selected_{channel}'));
                "
            />

            <button color="invisible" onclick="ShowOnly('main_lobby_chat_panel_{channel}'); ShowOnly('channel_header_selected_{channel}');"/>

            <panel color="invisible" x="0.5h" align="left" valign="center" width="{subwidth=-2.2h}" height="-1.0h" noclick="1">
                <label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="left" font="dyn_10" color="white" shadow="true" noclick="true" blockinput="false" shadowcolor="black">{channel_header}</label>
            </panel>

            <button
                x="-0.5h"
                width="1.7h" height="1.7h"
                align="right"
                valign="center"
                color="1 1 1 1"
                texture="/ui/info/close_button.tga"
                onshow="If({permanent}, SetVisible(false));"
                onclick="PlaySound('/ui/sounds/close.mp3'); if(!{permanent}, ChatLeaveChannel('{channel}'));"
            />

        </panel>
    </template>

    <template name="main_lobby_chat_channel">
        <panel name="main_lobby_chat_panel_{channel}" color="invisible" float="bottom" padding="0.5h" reverse="true" group="chat_channel_window" onshow="CreateString('_cur_lobby_channel', '{channel}');" watch="ChatLeftChannel" ontrigger="If(!{permanent} and (StringEmpty(param) or StringEquals(param, '{channel}')), DestroyWidget());" onload="If({startselected}, ShowOnly(name));">           
            <panel align="center" float="right" padding="1%" color="invisible" height="-1.6h">
                <panel width="-19.0h" noclick="1">
                    <frame texture="/ui/info/backdrop.tga" borderthickness="4.0h" color="1 1 1 .7" noclick="1" />                   
                    <!-- Chat Header Bar -->
                    <panel height="3.0h" align="center">
                        <instance name="header_bar" show_close="0"/>
                        <panel width="-1.0h" align="center" noclick="true" passivechildren="true">
                            <label y="0.1h" height="50%" width="-2.0h" valign="top" textalign="left" textvalign="center" color="white" shadow="1" font="dyn_12" noclick="1" onshow="SetText(Translate('{channel}'));"/>
                            <label y="-0.3h" height="40%" width="-2.0h" valign="bottom" textalign="left" textvalign="center" color="1 1 1 .8" shadow="1" font="dyn_9" noclick="1" watch="ChatChanTopic" ontrigger="if(StringEquals(param0, '{channel}'), If(!StringEquals(param1, ''), SetText(param1), ClearText()));"/>
                            
                            <label height="100%" width="20%" x="80%" textalign="right" textvalign="center" color="white" shadow="1" font="dyn_10" noclick="1" watch="ChatChanNumUsers" ontrigger="if(StringEquals(param0, '{channel}'), SetText(param1 # ' ' # Translate('general_users')));"/>
                        </panel>
                    </panel>
                    
                    <!-- Chat Buffer -->
                    <textbuffer
                        name="main_lobby_chat_buffer_{channel}"
                        scrolltexture="/ui/info/combobox.tga"
                        handleheight="2.0h"
                        y="-0.5h"
                        width="-1.0h" height="-3.9h"
                        align="center" valign="bottom"
                        scrollbarsize="2.0h"
                        textalign="left"
                        font="dyn_10"
                        wrap="true"
                        anchorbottom="true"
                        usesmileys="true"
                        maxlines="1000"
                        shadow="true"
                        color="invisible"
                        textcolor="white"
                        linktochat="true"
                        chatchannel="{channel}"
                        isgamechat="false"
                    />
                </panel>
                <panel width="18.0h" noclick="1">
                    <frame texture="/ui/info/backdrop.tga" borderthickness="4.0h" color="1 1 1 .7" noclick="1" />
                    <!-- Nicks in channel -->
                    <listbox
                        name="chat_{channel}_userbox_main"
                        y="1"
                        width="-2"
                        height="-1.0h"
                        align="center"
                        valign="top"
                        color="invisible"
                        font="dyn_10"
                        itemwidth="100%"
                        itemheight="2.3h"
                        highlight="under"
                        highlightcolor="#67a7da62"
                        highlightbordercolor="invisible"
                        select="true"
                        clearselection="true"
                        hoverselect="false"
                        exteriorscrollbars="false"
                        scrollbarplaceholder="false"
                        scrollbarsize="2.0h"
                        scrolltexture="/ui/info/combobox.tga"
                        handleheight="2.0h"

                        ondoubleclick="CreateString('_temp_name', this); Call('main_lobby_chat_box_{channel}', 'SetInputLine(\'/w \' # _temp_name # \' \'); SetFocus(true);');"

                        watch1="ChatUserEvent"
                        ontrigger1="If(StringEmpty(param0) or StringEquals(param0, '{channel}'), UICmd(param1));"
                        watch2="ChatUserNames"
                        ontrigger2="If(StringEmpty(param0) or StringEquals(param0, '{channel}'), Split(AddTemplateListItem('chat_list_entry', param1, 'name', param1, 'admin', param2, 'ingame', param3), SortListboxValue()));"
                        onrightclick="
                            CreateString('ui_lastSelectedUser', this);
                            CreateString('ui_tooltipTarget', 'lobby_userlist_rtclick_{channel}');
                            ShowWidget(ui_tooltipTarget);
                            Call(ui_tooltipTarget, 'SetX(\'' # param0 # '\');');
                            Call(ui_tooltipTarget, 'SetY(\'' # param1 # '\');');
                            PlaySound('/shared/sounds/ui/button_rightclick.mp3');"
                    />

                    <instance name="cc_rightclick_menu" menu_name="lobby_userlist_rtclick_{channel}" channel="{channel}"/>

                    <panel name="main_lobby_join_channel_{channel}" align="center" valign="center" color="0 0 0 1" cangrab="true" noclick="1" visible="false" onshow="ChatUpdateChannels();">
                        <instance name="header_bar" lbl="mainlobby_new_channel_header" height="3.0h" show_close="true" onclick="Call('main_lobby_join_channel_text_{channel}', 'EraseInputLine();'); Call('main_lobby_join_channel_{channel}', 'FadeOut(250);');"/>

                        <panel height="3.8h" y="3.0h" width="-2" align="center">
                            <frame texture="/ui/info/backdrop.tga" borderthickness="1.0h" color="1 1 1 .7" noclick="1" />
                            <panel x="0.5h" width="-7.5h" height="2.2h" valign="center" color="0 0 0 .5">
                                <frame color="invisible" bordercolor="#7b8c9b" borderthickness="1" noclick="1" />
                                <textbox
                                    name="main_lobby_join_channel_text_{channel}"
                                    y="0.3h"
                                    width="-1.0h"
                                    valign="center" align="center"
                                    maxlength="250"
                                    textalign="left" textvalign="center"
                                    font="dyn_10"
                                    textcolor="white"
                                    shadow="false"
                                    color="invisible"
                                    onshow="SetFocus(true);"
                                    onenter="
                                        PlaySound('/shared/sounds/ui/chat_send.wav');
                                        ChatJoinChannel(this);
                                        EraseInputLine();
                                        HideWidget('main_lobby_join_channel_{channel}');"
                                    onesc="EraseInputLine(); HideWidget('main_lobby_join_channel_{channel}');"
                                />
                            </panel>
                            <instance
                                name="button2"
                                x="-0.5h"
                                height="2.2h" width="6h"
                                align="right" valign="center"
                                font="dyn_9"
                                label="general_go"
                                onclick="Call('main_lobby_join_channel_text_{channel}', 'ChatJoinChannel(this); EraseInputLine();'); HideWidget('main_lobby_join_channel_{channel}');"
                            />
                        </panel>

                        <panel y="7.4h" width="-1.0h" height="-8.0h" align="center" color="invisible">
                            <table
                                name="chat_channel_list_{channel}"
                                headingcolor="#0d151e"
                                altrowcolor="#152334"
                                color="#1f334a"
                                rows="16"
                                expandable="true"
                                rowheight="2.0h"
                                cols="66%,34%"
                                font="dyn_10"
                                bordercolor="invisible"
                                headings="general_channel_name,general_users"
                                headingid="head"
                                align="center"
                                usescrollbar="true"
                                exteriorscrollbars="1"
                                scrolltexture="/ui/info/combobox.tga"
                                scrollbarsize="2.0h"
                                handleheight="2.0h"
                                watch="ChatChannelList"
                                onclick="CreateString('_channel_name', GetData(0, row)); if(!StringEquals(dataid, 'head'), Call('main_lobby_join_channel_text_{channel}', 'SetInputLine(_channel_name);'));"
                                ondoubleclick="Call('main_lobby_join_channel_text_{channel}', 'ChatJoinChannel(this); EraseInputLine();'); HideWidget('main_lobby_join_channel_{channel}');"
                                textalign="left"
                                textvalign="center"
                                autosort="false"
                            />
                        </panel>
                    </panel>


                </panel>
            </panel>

            <panel height="3.2h" align="center" color="invisible">
                <frame texture="/ui/info/backdrop.tga" borderthickness="1.0h" color="1 1 1 .7" noclick="1" />
                <panel width="-19.0h" height="2.6h" valign="center" color="0 0 0 .5">
                    <frame color="invisible" bordercolor="#7b8c9b" borderthickness="1" noclick="1" />
                    <!-- Chat text -->
                    <textbox
                        name="main_lobby_chat_box_{channel}"
                        y="0.5h"
                        width="-1.0h"
                        valign="center" align="center"
                        maxlength="250"
                        textalign="left" textvalign="center"
                        font="dyn_10"
                        textcolor="white"
                        shadow="false"
                        color="invisible"
                        onshow="SetFocus(true); SetInputLine(GetCurrentChatMessage());"
                        onkeydown="If(param == 281, ChatPrevHistory()); If(param == 283, ChatNextHistory()); If(param == 281 or param == 283, SetInputLine(ChatGetCurHistory()));"
                        onenter="
                            PlaySound('/shared/sounds/ui/chat_send.wav');
                            if(ChatSendMessage(this, '{channel}'), EraseInputLine());"
                        onesc="EraseInputLine(); SetFocus(true);"
                        onchange="SetCurrentChatMessage(this); if(StringLength(this) gt 0, EnableWidget('main_lobby_int_{channel}_send_message_button'), DisableWidget('main_lobby_int_{channel}_send_message_button'));"
                    />
                </panel>
                <instance
                    name="button2"
                    button_name="main_lobby_int_{channel}_send_message_button"
                    enabled="false"
                    height="2.4h" width="18.0h"
                    align="right" valign="center"
                    font="dyn_10"
                    label="general_send_message"
                    onclick="SetActiveWidget('main_lobby_chat_box_{channel}'); Call('main_lobby_chat_box_{channel}', 'if(ChatSendMessage(this, \'{channel}\'), EraseInputLine());');"
                />
            </panel>
        </panel>
    </template>

    <template name="friends_clan_lists">
        <panel height="3.8h">
            <instance name="tab_bar" />
            <panel width="-1.0h" align="center" color="invisible" noclick="1">
                <panel width="48%" align="left" height="70%" valign="center">
                    <frame visible="1" texture="/ui/info/button.tga" borderthickness="0.4h" color="white"/>
                    <frame name="tab_friends_lobby" group="tab_cc_lobby" texture="/ui/info/button_down.tga" borderthickness="0.4h" color="white"/>
                    <button color="invisible" onclick="ShowOnly('tab_friends_lobby'); ShowOnly('friends_lobby');"/>
                    <label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="dyn_10" color="white" shadow="true" shadowcolor="black" blockinput="false" noclick="true" content="mainlobby_label_cc_friends"/>
                </panel>
                <panel width="48%" align="right" height="70%" valign="center">
                    <frame visible="1" texture="/ui/info/button.tga" borderthickness="0.4h" color="white"/>
                    <frame name="tab_clan_lobby" group="tab_cc_lobby" texture="/ui/info/button_down.tga" borderthickness="0.4h" visible="false" color="white"/>
                    <button color="invisible" onclick="ShowOnly('tab_clan_lobby'); ShowOnly('clan_lobby');"/>
                    <label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="dyn_10" color="white" shadow="true" shadowcolor="black" blockinput="false" noclick="true" content="mainlobby_label_cc_clan"/>
                </panel>
            </panel>
        </panel>

        <panel y="4.2h" height="-4.2h" name="friends_lobby" group="lobby_cc">
            <frame texture="/ui/info/backdrop.tga" borderthickness="4.0h" color="1 1 1 .7" noclick="1"/>

            <instance name="button2" height="2.4h" y="-0.8h" width="80%" valign="bottom" align="center" font="dyn_10" color="white" label="main_lobby_add_friend" onclick="Call('main_lobby_add_buddy', 'FadeIn(250);');"/>

            <panel name="main_lobby_add_buddy" height="3.0h" y="-0.8h" width="-2.0h" valign="bottom" align="center" color="invisible" visible="0">
                <frame texture="/ui/info/backdrop.tga" borderthickness="1.0h" color="1 1 1 .7" noclick="1"/>

                <panel x="0.5h" width="-12.7h" height="2.2h" valign="center" color="0 0 0 .5">
                    <frame color="invisible" bordercolor="#7b8c9b" borderthickness="1" noclick="1" />
                    <textbox
                        name="main_lobby_add_friend_text"
                        y="0.4h"
                        width="-1.0h"
                        valign="center" align="center"
                        maxlength="250"
                        textalign="left" textvalign="center"
                        font="dyn_10"
                        textcolor="white"
                        shadow="false"
                        color="invisible"
                        onshow="SetFocus(true);"
                        onenter="
                            PlaySound('/shared/sounds/ui/chat_send.wav');
                            ChatAddBuddy(this);
                            EraseInputLine();
                            HideWidget('main_lobby_add_buddy');"
                        onesc="EraseInputLine(); HideWidget('main_lobby_add_buddy');"
                    />
                </panel>
                <instance
                    name="button2"
                    x="-2.7h"
                    height="2.2h" width="8.8h"
                    align="right" valign="center"
                    font="dyn_10"
                    label="general_add_friend"
