				ontrigger1="If(IsVisible(), DoEvent(1));"
				onevent1="
					## Echo('BangSpecUIThrottle Sorting'); ##
					Set('specui_player_{tab}0_preSortString', '');
					Set('specui_player_{tab}1_preSortString', '');
					Set('specui_graph_max_{tab}', 0);
					
					For(0, 4, 1, '
						Set(\'specui_player_{tab}0_preSortString\', Evaluate(\'specui_player_{tab}0_preSortString\') # Evaluate(\'specui_player_{tab}0_\'#iParam) # \',\' # iParam );
						If(iParam lt 4, Set(\'specui_player_{tab}0_preSortString\', Evaluate(\'specui_player_{tab}0_preSortString\') # \'|\' ));
						If( Evaluate(\'specui_player_{tab}0_\'#iParam) gt Evaluate(\'specui_graph_max_{tab}\'), Set(\'specui_graph_max_{tab}\', Evaluate(\'specui_player_{tab}0_\'#iParam)) );					
					');
					
					For(5, 9, 1, '
						Set(\'specui_player_{tab}1_preSortString\', Evaluate(\'specui_player_{tab}1_preSortString\') # Evaluate(\'specui_player_{tab}1_\'#iParam) # \',\' # iParam );
						If(iParam lt 9, Set(\'specui_player_{tab}1_preSortString\', Evaluate(\'specui_player_{tab}1_preSortString\') # \'|\' ));
						If( Evaluate(\'specui_player_{tab}1_\'#iParam) gt Evaluate(\'specui_graph_max_{tab}\'), Set(\'specui_graph_max_{tab}\', Evaluate(\'specui_player_{tab}1_\'#iParam)) );
					');										
					
					Set('specui_player_{tab}1_postSortString', Sort(Evaluate('specui_player_{tab}1_preSortString'), '|', 0, ',', 0, 1));
					Set('specui_player_{tab}0_postSortString', Sort(Evaluate('specui_player_{tab}0_preSortString'), '|', 1, ',', 0, 1));
					
					ExplodeTrigger('SpecUISort{tab}0', Evaluate('specui_player_{tab}0_postSortString'), '|', ',');
					ExplodeTrigger('SpecUISort{tab}1', Evaluate('specui_player_{tab}1_postSortString'), '|', ',');
				"				
			>				
				<instance name="specui_graph_bar" tab="{tab}" team="0" index="0" value="50" id="specui_{tab}_bars" width="3h" height="1%" color="0.3 1 0.2 1.0" paramtrigger="{paramtrigger}" div="{div=0}" label="{label}"   />
				<instance name="specui_graph_bar" tab="{tab}" team="0" index="1" value="50" id="specui_{tab}_bars" width="3h" height="1%" color="0.3 1 0.2 1.0" paramtrigger="{paramtrigger}" div="{div=0}" label="{label}"   />
				<instance name="specui_graph_bar" tab="{tab}" team="0" index="2" value="50" id="specui_{tab}_bars" width="3h" height="1%" color="0.3 1 0.2 1.0" paramtrigger="{paramtrigger}" div="{div=0}" label="{label}"   />
				<instance name="specui_graph_bar" tab="{tab}" team="0" index="3" value="50" id="specui_{tab}_bars" width="3h" height="1%" color="0.3 1 0.2 1.0" paramtrigger="{paramtrigger}" div="{div=0}" label="{label}"   />
				<instance name="specui_graph_bar" tab="{tab}" team="0" index="4" value="50" id="specui_{tab}_bars" width="3h" height="1%" color="0.3 1 0.2 1.0" paramtrigger="{paramtrigger}" div="{div=0}" label="{label}"   />			
			</panel>
			
			<panel noclick="1" x="21%" width="40%" float="right" valign="bottom" align="center" padding="1h" color="0.3 0.3 0.3 0.0" 
				onhide="SetHeight('1%'); SleepWidget(0, '');" 
				onshow="SetHeight('1%'); ScaleHeight('1%', 1); DoEvent();"
				onload="CreateString('specui_player_{tab}1_preSortString'); CreateString('specui_player_{tab}1_postSortString');"
				onevent=" ScaleHeight('80%', 350);"
			>				
				<instance name="specui_graph_bar" tab="{tab}" team="1" index="5" value="50" id="specui_{tab}_bars" width="3h" height="1%" color="red" paramtrigger="{paramtrigger}" div="{div=0}" label="{label}"   />
				<instance name="specui_graph_bar" tab="{tab}" team="1" index="6" value="50" id="specui_{tab}_bars" width="3h" height="1%" color="red" paramtrigger="{paramtrigger}" div="{div=0}" label="{label}"   />
				<instance name="specui_graph_bar" tab="{tab}" team="1" index="7" value="50" id="specui_{tab}_bars" width="3h" height="1%" color="red" paramtrigger="{paramtrigger}" div="{div=0}" label="{label}"   />
				<instance name="specui_graph_bar" tab="{tab}" team="1" index="8" value="50" id="specui_{tab}_bars" width="3h" height="1%" color="red" paramtrigger="{paramtrigger}" div="{div=0}" label="{label}"   />
