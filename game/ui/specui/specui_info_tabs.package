				<label fity="true" width="100%" align="center" valign="center" textalign="center" textvalign="center" noclick="true" font="dyn_10" color="white" wrap="true" shadow="true" >{lbl}</label>
			</panel>
			<panel noclick="1" height="2.0h" align="right" x="-0.5h" y="-0.3h" valign="center" onshow="Call('specui_keybind_{id}', 'SetVisible(specui_show_hotkeys);');">
