	<template name="specui_graph_bar" >
		<frame group="specui_{id}_graph_bars" onevent="DestroyWidget();" noclick="0" width="{width=3h}" color="{color=orange}" valign="bottom" height="{height=1%}" visible="0"
			onmouseover="SetColor('cyan');"  
			onmouseout="SetColor('{color=orange}'); HideWidget('specui_graph_tooltips');" 
			onhide="SetColor('{color=orange}'); HideWidget('specui_graph_tooltips');"
			bordercolor="0 0 0 0.6"  borderthickness="1"
			watch="SpecUISort{tab}{team}"
			ontrigger="			
				## Echo('SpecUISort{tab}{team} Sorting'); ##
				If(({index} == param2),
					SetHeight( 
						Max (( (param1 / specui_graph_max_{tab}) * 100), 3) # '%' 
					)
				);	
				If(({index} == param2),
					If( (GetX() != (1-param0) * -1 * GetXFromString('4.0h')), Split(SlideX((1-param0) * -1 * GetXFromString('4.0h'), 250), SleepWidget(300, 'SetX(GetX());')) )
				);
				" 
			watch1="SpectatorHeroPlayerInfo{index}"
			ontrigger1="
				SetOnMouseOver('
					Trigger(\'SpecUIGraphTooltipUpdate\', \''# param0 #'\', \''# param1 #'\', \'specui_player_{tab}{team}_{index}\', \'{label}\', \'{div=0}\');
					ShowWidget(\'specui_graph_tooltips\');
				');
				SetVisible(!StringEmpty(param1));
			"	
			watch2="SpectatorHeroExists{index}" ontrigger2="SetVisible(!StringEmpty(SpectatorPlayer{index}) and SpectatorHeroExists{index});"
			onclick="SelectUnit(SpectatorHeroIndex{index});"
			onrightclick="If(GetSelectedEntity() != SpectatorHeroIndex{index}, Set(cg_lockCamera, 1), Set(cg_lockCamera, !cg_lockCamera)); SelectUnit(SpectatorHeroIndex{index}); "	
		>
			<panel y="0" x="0" width="3h" height="4.7h" visible="0"  noclick="0" onload="SetY('-4.8h'); CreateInt('specui_player_{tab}{team}_{index}');" 
				watch1="SpectatorPlayer{index}" ontrigger1="Set('specui_player_{tab}{team}_{index}', {paramtrigger});"
				watch="SpectatorHeroPlayerInfo{index}" ontrigger="				
					SetVisible(!StringEmpty(param1));
					SetOnMouseOver('
						Trigger(\'SpecUIGraphTooltipUpdate\', \''# param0 #'\', \''# param1 #'\', \'specui_player_{tab}{team}_{index}\', \'{label}\', \'{div=0}\');
						ShowWidget(\'specui_graph_tooltips\');
					');					
					"					
				onmouseout="HideWidget('specui_graph_tooltips');" 
				onhide="HideWidget('specui_graph_tooltips');"
				onclick="SelectUnit(SpectatorHeroIndex{index});"
				onrightclick="If(GetSelectedEntity() != SpectatorHeroIndex{index}, Set(cg_lockCamera, 1), Set(cg_lockCamera, !cg_lockCamera)); SelectUnit(SpectatorHeroIndex{index}); "			
			>
				<!-- Hero Icon -->
				<panel width="3h" height="3h" noclick="1" color="white" valign="top"  align="center"
					watch="SpectatorHeroPlayerInfo{index}" ontrigger="SetColor(param1);"					
				>
					<image  height="92%" width="92@"  noclick="1" valign="center" align="center"  
							onload="SetTexture(GetEntityIconPath('Hero_Ophelia'));"
							watch="SpectatorHeroHeroInfo{index}" ontrigger="SetTexture(param1);"
							watch1="SpectatorHeroStatus{index}" ontrigger1="If(param, SetRenderMode('normal'), SetRenderMode('grayscale'));" 
					/>	
				</panel>
				<label height="1.7h" y="1" width="3.6h" align="center" valign="bottom" textalign="center" textvalign="bottom" font="dyn_11" color="#ffab01" content="{index}" noclick="1" outline="1"
					watch1="SpectatorHeroPlayerInfo{index}" ontrigger1="If({div=0} != 0, SetText(''), SetText(Evaluate('specui_player_{tab}{team}_{index}'))); SetColor(If({team} == 0, '0.3 1 0.2 1.0', '1.0 0.3 0.0 1'));"
					watch="SpectatorPlayer{index}" ontrigger="If({div=0} == 0, SetText({paramtrigger}), If(Round({paramtrigger}*100/{div=0}) lt 101, SetText(Round({paramtrigger}*100/{div=0})#'%'), SetText('')));"
				/>					
			</panel>
			
		</frame>
	</template>
	
	<template name="specui_stats_graph" >	
		<panel noclick="1" x="-1.2h" y="-0.7h" width="49h" height="80%" align="center" valign="bottom" group="specui_{tab}_graph_bars"  onevent1="SetVisible(1);" onevent2="SetVisible(0);" onevent3="SetVisible(0);"  >
			<panel noclick="1" x="-21%" width="40%" float="right" valign="bottom" align="center" padding="1h" color="0.3 0.3 0.3 0.0" 
				onhide="SetHeight('1%'); SleepWidget(0, '');" 
				onshow="SetHeight('1%'); ScaleHeight('1%', 1); DoEvent(); Trigger('BangSpecUIThrottleToggle', 1); SleepWidget(400, 'DoEvent(1);');"
				onload="CreateString('specui_player_{tab}0_preSortString'); CreateString('specui_player_{tab}0_postSortString'); CreateFloat('specui_graph_max_{tab}', 0);"
				onevent="ScaleHeight('80%', 350);"
