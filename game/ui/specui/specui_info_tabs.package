<?xml version="1.0" encoding="UTF-8"?>
<package>

	<template name="specui_datarow_sep">
		<panel width="100%" height="0.3h" noclick="true" sticky="1" stickytoinvis="0"
			watch="SpectatorHeroExists{index}" ontrigger="SetVisible(!StringEmpty(SpectatorPlayer{index}) and SpectatorHeroExists{index});"
		>
			<panel color=".15 .15 .15 1" y="-1" width="-1.0h" height="1" valign="center" noclick="true" />
			<panel color="0 0 0 1" width="-1.0h" height="1" valign="center" noclick="true" />
		</panel>
	</template>

	<template name="specui_stats_slot_header">
		<panel width="100%" height="2.4h" padding="0.1h" noclick="1" sticky="1" stickytoinvis="0" color="1 0 1 0.0" >	
			<frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="0.6h" color="{headercolor=.5 .5 .5 1}"  onload="If(specui_mini_mode1, If(StringEquals('{headercolor=.5 .5 .5 1}', '#460000CB'), Split(SetBorderColor('#46000066'), SetColor('#46000066')), Split(SetBorderColor('#123d2566'), SetColor('#123d2566'))));"/>
			<frame x="0" texture="/ui/frames/roundframe.tga" color="0 0 0 1" borderthickness="0.6h" blockinput="false" noclick="true"/>	
			<panel float="right" padding="0.1h" noclick="true">			
			
				<!-- Spacer -->
				<panel width="3.8h" noclick="1" sticky="1" stickytoinvis="0"></panel>
				<!-- Murder Death Kills -->
				<panel width="8h" height="90%" noclick="0" onmouseover="Trigger('SpecUITooltipUpdate', 'specui_kda', 'specui_kda_tip', '/ui/elements/stat_icons/kill_death_ratio.tga', '', '', '');" onmouseout="Trigger('SpecUITooltipUpdate', '', '', '', '', '', '');" sticky="1" stickytoinvis="0" color="invisible" >
					<image x="0.0" y="0.1h" width="1.7h" height="1.7h" align="center" valign="center" noclick="1" visible="1" texture="/ui/elements/stat_icons/kill_death_ratio.tga" />
				</panel>
				<!-- XPPM -->
				<panel width="4.0h" height="90%" noclick="0" onmouseover="Trigger('SpecUITooltipUpdate', 'specui_xp', 'specui_xp_tip', '/ui/specui/elements/PushMarkerExp.tga', '', '', '');" onmouseout="Trigger('SpecUITooltipUpdate', '', '', '', '', '', '');" sticky="1" stickytoinvis="0" color="invisible" >
					<image x="0.0" width="2.5h" height="2.5h" align="center" valign="center" noclick="1" visible="1" texture="/ui/specui/elements/PushMarkerExp.tga" />
				</panel>
				<!-- GPM -->
				<panel width="4.5h" height="90%" noclick="0" onmouseover="Trigger('SpecUITooltipUpdate', 'specui_gm', 'specui_gm_tip', '/ui/specui/elements/PushMarkerGold.tga', '', '', '');" onmouseout="Trigger('SpecUITooltipUpdate', '', '', '', '', '', '');" sticky="1" stickytoinvis="0" color="invisible" >
					<image x="0.0" width="1.8h" height="1.8h" align="center" valign="center" noclick="1" visible="1" texture="/ui/specui/elements/PushMarkerGold.tga" />
				</panel>		

				<!-- Creep Kills -->
				<panel width="3.8h" height="90%" noclick="0" onmouseover="Trigger('SpecUITooltipUpdate', 'specui_ck', 'specui_ck_tip', '/ui/elements/stat_icons/npc_kills.tga', '', '', '');" onmouseout="Trigger('SpecUITooltipUpdate', '', '', '', '', '', '');" sticky="1" stickytoinvis="0" color="invisible" >
					<image x="0.0" y="0.1h" width="1.7h" height="1.7h" align="center" valign="center" noclick="1" visible="1" texture="/ui/elements/stat_icons/npc_kills.tga" />
				</panel>				
			
				<!-- Creep Denies -->
				<panel width="3.8h" height="90%" noclick="0" onmouseover="Trigger('SpecUITooltipUpdate', 'specui_cd', 'specui_cd_tip', '/ui/elements/stat_icons/npc_kills.tga', '', '', '');" onmouseout="Trigger('SpecUITooltipUpdate', '', '', '', '', '', '');" sticky="1" stickytoinvis="0" color="invisible" >
					<image x="0.0" y="0.1h" width="1.7h" height="1.7h" align="center" valign="center" noclick="1" visible="1" texture="/ui/elements/stat_icons/npc_kills.tga" color="red" />
				</panel>	
			
				<!-- Player Dmg -->
				<panel width="4.1h" height="90%" noclick="0" onmouseover="Trigger('SpecUITooltipUpdate', 'specui_hdmg', 'specui_hdmg_tip', '/ui/elements/stat_icons/player_damage.tga', '', '', '');" onmouseout="Trigger('SpecUITooltipUpdate', '', '', '', '', '', '');" sticky="1" stickytoinvis="0" color="invisible" >
					<image x="0.0" y="0.1h" width="1.7h" height="1.7h" align="center" valign="center" noclick="1" visible="1" texture="/ui/elements/stat_icons/player_damage.tga" />
				</panel>				

				<!-- Building Dmg -->
				<panel width="4.2h" height="90%" noclick="0" onmouseover="Trigger('SpecUITooltipUpdate', 'specui_bdmg', 'specui_bdmg_tip', '/ui/elements/stat_icons/razed.tga', '', '', '');" onmouseout="Trigger('SpecUITooltipUpdate', '', '', '', '', '', '');" sticky="1" stickytoinvis="0" color="invisible" >
					<image x="0.0" y="0.1h" width="1.7h" height="1.7h" align="center" valign="center" noclick="1" visible="1" texture="/ui/elements/stat_icons/razed.tga" />
				</panel>
			
			</panel>
			
		</panel>
	</template>		
	<template name="specui_stats_slot">
		<panel width="100%" height="2.4h" float="right" padding="0.1h" noclick="1" sticky="1" stickytoinvis="0"
			watch="SpectatorHeroExists{index}" ontrigger="SetVisible(!StringEmpty(SpectatorPlayer{index}) and SpectatorHeroExists{index});"
		>	
			<!-- Level -->
			<label width="2.1h" x="-0.6h" textvalign="center" textalign="center" font="dyn_11" color="white" shadow="1" watch="SpectatorPlayer{index}" ontrigger="SetText(param4);" noclick="1"/>
			<!-- Icon -->
			<frame width="100@" height="100%" align="left" valign="center" watch="SpectatorPlayer{index}" ontrigger="SetColor(param3);" noclick="1"
			>
				<image width="-2" height="-2" align="center" valign="center" watch="SpectatorPlayer{index}" ontrigger="SetTexture(param2);"  noclick="0"
					onclick="SelectUnit(SpectatorHeroIndex{index});"
					onrightclick="If(GetSelectedEntity() != SpectatorHeroIndex{index}, Set(cg_lockCamera, 1), Set(cg_lockCamera, !cg_lockCamera)); SelectUnit(SpectatorHeroIndex{index}); "					
				/>
			</frame> 

			<!-- Murder Death Kills -->
			<panel width="7.6h" height="90%" noclick="1" sticky="1" stickytoinvis="0" color="invisible" >
				<label textvalign="center" textalign="center" font="dyn_11" color="0.3 1.0 .3 1" shadow="1" watch="SpectatorPlayer{index}" ontrigger="SetText('^g' # param5 # '^777/' # '^r' # param6 # '^777/' # '^y' # param7)" noclick="1"/>
			</panel>
			<!-- XPPM -->
			<panel width="4.0h" height="90%" noclick="1" sticky="1" stickytoinvis="0" color="invisible" >
				<label textvalign="center" textalign="center" font="dyn_11" color="0.7 0.5 1 1" shadow="1" watch="SpectatorPlayer{index}" ontrigger="SetText(Ceil(param9));" noclick="1"/>
			</panel>
			<!-- GPM -->
			<panel width="4.5h" height="90%" noclick="1" sticky="1" stickytoinvis="0" color="invisible" >
				<label textvalign="center" textalign="center" font="dyn_11" color="1 0.74 0 1" shadow="1" watch="SpectatorPlayer{index}" ontrigger="SetText(Ceil(param10));" noclick="1"/>
			</panel>		
			
			<!-- Creep Kills -->
			<panel width="3.8h" height="90%" noclick="1" sticky="1" stickytoinvis="0" color="invisible" >
				<label textvalign="center" textalign="center" font="dyn_11" color="0.7 1.0 0.7 1" shadow="1" watch="SpectatorPlayer{index}" ontrigger="SetText(Ceil(param15));" noclick="1"/>
			</panel>			
			
			<!-- Creep Denies -->
			<panel width="3.8h" height="90%" noclick="1" sticky="1" stickytoinvis="0" color="invisible" >
				<label textvalign="center" textalign="center" font="dyn_11" color="1.0 0.7 0.7 1" shadow="1" watch="SpectatorPlayer{index}" ontrigger="SetText(Ceil(param16));" noclick="1"/>
			</panel>				
			
			<!-- Player Dmg -->
			<panel width="4.2h" height="90%" noclick="1" sticky="1" stickytoinvis="0" color="invisible" onload="CreateFloat('specui_state_playerDamage_{index}', 0);" >
				<label textvalign="center" textalign="center" font="dyn_11" color="0.7 1.0 0.7 1" shadow="1"  noclick="1" 
					watch="SpectatorPlayer{index}" 
					ontrigger="
					CallEvent('specui_brain_bug', 2);
					Set('specui_state_playerDamage_{index}', param13); If(specui_state_playerDamage_max gt 1 and FtoA(param13*100/specui_state_playerDamage_max, 0, 0) lt 101, SetText(FtoA(param13*100/specui_state_playerDamage_max, 0, 0)#'%'), SetText('-'));" 
				/>
			</panel>				
			
			<!-- Building Dmg -->
			<panel width="4.2h" height="90%" noclick="1" sticky="1" stickytoinvis="0" color="invisible" onload="CreateFloat('specui_state_buildingDamage_{index}', 0);" >
				<label textvalign="center" textalign="center" font="dyn_11" color="1.0 0.7 0.7 1" shadow="1"  noclick="1" 
					watch="SpectatorPlayer{index}" 
					ontrigger="
					CallEvent('specui_brain_bug', 3);
					Set('specui_state_buildingDamage_{index}', param14); If(specui_state_buildingDamage_max gt 1 and FtoA(param14*100/specui_state_buildingDamage_max, 0, 0) lt 101, SetText(FtoA(param14*100/specui_state_buildingDamage_max, 0, 0)#'%'), SetText('-'));" 
				/>
			</panel>				
			
		</panel>
	</template>			
	 
