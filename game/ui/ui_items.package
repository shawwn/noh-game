	<template name="cc_rightclick_menu">
		<instance name="rightclick_menu" menu_name="{menu_name}" numitems="17"
			item1value="ChatOpenMessage(ui_lastSelectedUser);" item1label="ui_items_cc_right_click_send_message" item1condition="!StringEquals(GetAccountName(), ui_lastSelectedUser) and ChatUserOnline(ui_lastSelectedUser)"
			item2value="ChatJoinGame(ui_lastSelectedUser);" item2label="ui_items_cc_right_click_joing_game" item2condition="ChatUserInGame(ui_lastSelectedUser) and !StringEquals(GetAccountName(), ui_lastSelectedUser)"
			item3value="ChatOpenClanMessage();" item3label="ui_items_cc_right_click_message_clan" item3condition="IsInClan() and IsClanMember(ui_lastSelectedUser)"
			item4value="GetPlayerStatsName(ui_lastSelectedUser); ShowCCPanel(); ShowCCStatistics(true);" item4label="ui_items_cc_right_click_view_stats"
			item5value="ChatAddBuddy(ui_lastSelectedUser);" item5label="ui_items_cc_right_click_add_buddy" item5condition="!IsBuddy(ui_lastSelectedUser) and !StringEquals(GetAccountName(), ui_lastSelectedUser)"
			item6value="ChatRemoveBuddy(ui_lastSelectedUser);" item6label="ui_items_cc_right_click_remove_buddy" item6condition="IsBuddy(ui_lastSelectedUser) and !StringEquals(GetAccountName(), ui_lastSelectedUser)"
			item7value="GameInvite(ui_lastSelectedUser);" item7label="ui_items_cc_right_click_invite_to_game" item7condition="((IsPrivateGame() and IsHost()) or !IsPrivateGame()) and !StringEquals(GetAccountName(), ui_lastSelectedUser) and ChatUserOnline(ui_lastSelectedUser) and ChatInGame() and !ChatUserInGame(ui_lastSelectedUser)"
			item8value="GetUserInfo(ui_lastSelectedUser);" item8label="ui_items_cc_right_click_user_info" item8condition="!StringEquals(GetAccountName(), ui_lastSelectedUser)"
			item9value="ChatChannelKick(ui_lastSelectedUser, '{channel}');" item9label="ui_items_cc_right_click_channel_kick" item9condition="!StringEquals(GetAccountName(), ui_lastSelectedUser) and ChatGetAdminLevel(GetAccountName(), '{channel}') gt ChatGetAdminLevel(ui_lastSelectedUser, '{channel}')"
			item10value="ChatChannelBan(ui_lastSelectedUser, '{channel}');" item10label="ui_items_cc_right_click_channel_ban" item10condition="!StringEquals(GetAccountName(), ui_lastSelectedUser) and ChatGetAdminLevel(GetAccountName(), '{channel}') gt ChatGetAdminLevel(ui_lastSelectedUser, '{channel}')"
			item11value="ChatChannelSilence(ui_lastSelectedUser, '{channel}', 5);" item11label="ui_items_cc_right_click_channel_silence" item11condition="!StringEquals(GetAccountName(), ui_lastSelectedUser) and ChatGetAdminLevel(GetAccountName(), '{channel}') gt ChatGetAdminLevel(ui_lastSelectedUser, '{channel}')"
			item12value="ChatIgnore(ui_lastSelectedUser);" item12label="ui_items_cc_right_click_ignore" item12condition="!StringEquals(GetAccountName(), ui_lastSelectedUser) and !ChatIsIgnored(ui_lastSelectedUser)"
			item13value="ChatUnignore(ui_lastSelectedUser);" item13label="ui_items_cc_right_click_unignore" item13condition="!StringEquals(GetAccountName(), ui_lastSelectedUser) and ChatIsIgnored(ui_lastSelectedUser)"
			item14value="ChatAddBanlist(ui_lastSelectedUser, 'Unknown');" item14label="ui_items_cc_right_click_addban" item14condition="!StringEquals(GetAccountName(), ui_lastSelectedUser) and !ChatIsBanned(ui_lastSelectedUser)"
			item15value="ChatRemoveBanlist(ui_lastSelectedUser);" item15label="ui_items_cc_right_click_remban" item15condition="!StringEquals(GetAccountName(), ui_lastSelectedUser) and ChatIsBanned(ui_lastSelectedUser)"
			item16value="ChatChannelPromote(ui_lastSelectedUser, '{channel}');" item16label="ui_items_cc_right_click_channel_promote" item16condition="!StringEquals(GetAccountName(), ui_lastSelectedUser) and ChatGetAdminLevel(GetAccountName(), '{channel}') gt (ChatGetAdminLevel(ui_lastSelectedUser, '{channel}') + 1)"
			item17value="ChatChannelDemote(ui_lastSelectedUser, '{channel}');" item17label="ui_items_cc_right_click_channel_demote" item17condition="!StringEquals(GetAccountName(), ui_lastSelectedUser) and ChatGetAdminLevel(GetAccountName(), '{channel}') gt ChatGetAdminLevel(ui_lastSelectedUser, '{channel}') and ChatIsAdmin(ui_lastSelectedUser, '{channel}')"
		/>
	</template>
	
