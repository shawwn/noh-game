<?xml version="1.0" encoding="UTF-8"?>
<package>
	<!-- Textures -->
	<texture file="/ui/info/backdrop_b.tga" nocompress="true" />
	<texture file="/ui/info/backdrop_bl.tga" nocompress="true" />
	<texture file="/ui/info/backdrop_br.tga" nocompress="true" />
	<texture file="/ui/info/backdrop_c.tga" nocompress="true" />
	<texture file="/ui/info/backdrop_l.tga" nocompress="true" />
	<texture file="/ui/info/backdrop_r.tga" nocompress="true" />
	<texture file="/ui/info/backdrop_t.tga" nocompress="true" />
	<texture file="/ui/info/backdrop_tl.tga" nocompress="true" />
	<texture file="/ui/info/backdrop_tr.tga" nocompress="true" />
	
	<texture file="/ui/info/combobox_dropdown_b.tga" nocompress="true" />
	<texture file="/ui/info/combobox_dropdown_bl.tga" nocompress="true" />
	<texture file="/ui/info/combobox_dropdown_br.tga" nocompress="true" />
	<texture file="/ui/info/combobox_dropdown_c.tga" nocompress="true" />
	<texture file="/ui/info/combobox_dropdown_l.tga" nocompress="true" />
	<texture file="/ui/info/combobox_dropdown_r.tga" nocompress="true" />
	<texture file="/ui/info/combobox_dropdown_t.tga" nocompress="true" />
	<texture file="/ui/info/combobox_dropdown_tl.tga" nocompress="true" />
	<texture file="/ui/info/combobox_dropdown_tr.tga" nocompress="true" />

	<!-- Templates -->
	<template name="main_login_checkbox_state">
		<widgetstate statename="{state}">		
			<image width="5.4h" height="2.7h" align="{align}" valign="bottom" texture="/ui/mainmenu/{image}.tga"/>			
			<image width="5.4h" height="2.7h" align="{align}" valign="bottom" visible="{checked}" texture="/ui/mainmenu/{image}_on.tga"/>
			<label x="{textoffset}" y="0.1h" height="100%" width="100%" textalign="{align}" textvalign="center" font="{font}" color="{color}" shadow="true" noclick="1">{label}</label>			
		</widgetstate>
	</template>
	
	<template name="main_login_checkbox">
		<panel noclick="true" color="invisible">
			<button 
				color="invisible"
				numstates="2"
				cvar="{cvar}"
				onclick="PlaySound('/shared/sounds/ui/button_click_04.wav'); {onclick}"
			>
				<instance name="main_login_checkbox_state" state="up" label="{label}" align="{align}" textoffset="{textoffset}" image="{image}" font="{font=dyn_9}" color="#e0e0e0" checked="false"/>
				<instance name="main_login_checkbox_state" state="over" label="{label}" align="{align}" textoffset="{textoffset}" image="{image}" font="{font=dyn_9}" color="#3abde7" checked="false"/>
				<instance name="main_login_checkbox_state" state="down" label="{label}" align="{align}" textoffset="{textoffset}" image="{image}" font="{font=dyn_9}" color="#3abde7" checked="false"/>
				<instance name="main_login_checkbox_state" state="up2" label="{label}" align="{align}" textoffset="{textoffset}" image="{image}" font="{font=dyn_9}" color="#ffaa00" checked="true"/>
				<instance name="main_login_checkbox_state" state="over2" label="{label}" align="{align}" textoffset="{textoffset}" image="{image}" font="{font=dyn_9}" color="#3abde7" checked="true"/>
				<instance name="main_login_checkbox_state" state="down2" label="{label}" align="{align}" textoffset="{textoffset}" image="{image}" font="{font=dyn_9}" color="#3abde7" checked="true"/>
			</button>
		</panel>
	</template>

	<template name="Ncombobox_item">
		<label height="100%" x="50@" font="dyn_9" color="white" textvalign="center" shadow="true" shadowoffset="1" content="{label}"/>
	</template>
	
	<template name="combobox_item_icon">
		<image width="100@" height="100%" x="35@" texture="{texture}"/>
		<label height="100%" x="160@" font="dyn_9" color="{color=white}" textvalign="center" shadow="true" shadowoffset="1" content="{label}"/>
	</template>

	<template name="Noptions_combobox_state">
		<widgetstate statename="{state}">
			<panel color="invisible">
				<image height="100%" width="85%" align="center" texture="/ui/info/combobox_background_c.tga" color="{color}"/>
				<image height="100%" width="100@" align="left" texture="/ui/info/combobox_background_l.tga" color="{color}"/>
				<image height="100%" width="100@" align="right" texture="/ui/info/combobox_background_r.tga" color="{color}"/>
				<image x="-1" width="100@" height="100%" align="right" valign="center" texture="/ui/info/combobox_button_{image}.tga" />
			</panel>
		</widgetstate>
	</template> 
	
	<template name="Noptions_combobox">
		<combobox
			name="{id=}"
			x="{x}"
			width="{width}"
			height="2h"
			align="{align=right}"
			valign="{valign=center}"
			color="invisible"
			itemwidth="{itemwidth}"
			itemheight="2h"
			maxlistheight="{maxlistheight}"
			cvar="{cvar}"
			exteriorscrollbars="false"
			scrolltexture="/ui/info/combobox.tga"
			handleheight="100@" handlewidth="100%"
			scrollbarsize="2h"
			scrollbaroffset="0"
			highlight="under"
			onload="ComboBoxCmd('clear'); {populate=}; {data}('{itemtemplate=Ncombobox_item}'); {onload=}"
			onselect="{onselect=}"
			onshow="{onshow=}"
			onmouseover="PlaySound('/shared/sounds/ui/button_over_02.wav');"
			watch="{watch=}"
			ontrigger="{ontrigger=}"
			visible="{visible=true}"
			form="{form=}" data="{formdata=}"
		>
			<instance name="Noptions_combobox_state" state="up" color="1 1 1 1" image="up"/>
			<instance name="Noptions_combobox_state" state="over" color="#69ccff" image="over"/>
			<instance name="Noptions_combobox_state" state="down" color="#69ccff" image="over"/>
			<widgetstate statename="listbg">
				<frame height="+2.7h" y="-1.9h" texture="/ui/info/combobox_dropdown.tga" borderthickness="3h" color="white" />
			</widgetstate>
			<widgetstate statename="itembg">
				<panel width="{itemwidth}" />
			</widgetstate>
			<widgetstate statename="itemhighlight">
				<panel width="{itemwidth}" texture="/ui/info/header_bar_c.tga">
					<panel width="100%" height="100%" >
						<image height="50%" width="32" align="left" valign="top" texture="/ui/info/header_bar_tl.tga" noclick="1"/>
						<image height="50%" width="32" align="left" valign="bottom" texture="/ui/info/header_bar_bl.tga" noclick="1"/>
						<image height="50%" width="32" align="right" valign="top" texture="/ui/info/header_bar_tr.tga" noclick="1"/>
						<image height="50%" width="32" align="right" valign="bottom" texture="/ui/info/header_bar_br.tga" noclick="1"/>		
					</panel>
				</panel>
			</widgetstate>
		</combobox>
	</template>
	
	<template name="Noptions_slider">
		<panel width="{width=100%}" height="100%" align="center" color="invisible">				
			<panel height="100%" width="38%" float="bottom">
			
			<cvarlabel
				width="100%" height="50%"
				textvalign="center" textalign="right"
				font="dyn_9"
				color="white"
				shadow="true"
				prefix="{label}"
				onload="CreateString('{slider_name}_value');"
				cvar="{slider_name}_value"
			/>
			
			<cvarlabel
				width="100%" height="50%"
				textvalign="center" textalign="right"
				font="dyn_8"
				color="#3abde7"
				shadow="true"
				onload="CreateString('{slider_name}_value');"
				cvar="{slider_name}_value"
			/>			
				
			</panel>

			<panel width="62%" height="100%" valign="center" align="right" color="invisible">
				<panel width="96%" height="100%" valign="center" align="right" color="invisible">
					<image width="84%" align="center" texture="/ui/info/slider_backdrop_c.tga" />							
					<image height="100%" width="100@" align="left" texture="/ui/info/slider_backdrop_l.tga" />
					<image height="100%" width="100@" align="right" texture="/ui/info/slider_backdrop_r.tga" />

					<panel height="100%" width="15%" color="invisible" valign="center">
						<label width="100%" height="100%" textalign="center" textvalign="center" align="center" font="dyn_8" color="white" shadow="true">ui_items_slider_low</label>
					</panel>

					<panel height="2h" width="70%" align="center" color="invisible" valign="center">

						<slider
							name="{slider_name}"
							slotwidth="-1" slotheight="0.6h"
							slotcolor="1 1 1 1"
							handlewidth="100@" handleheight="100%"
							handlecolor="1 1 1 1"
							cvar="{cvar}"
							minvalue="{minvalue}" maxvalue="{maxvalue}"
							step="{step}"
							texture="/ui/info/slider.tga"
							onchange="Set({slider_name}_value, {value}); {action=};"
							onshow="Set({slider_name}_value, {value});"
						/>
					</panel>

					<panel height="100%" width="15%" align="right" color="invisible" valign="center">
						<label width="100%" height="100%" textalign="center" textvalign="center" align="center" font="dyn_8" color="white" shadow="true">ui_items_slider_high</label>
					</panel>

				</panel>
			</panel>
		</panel>
	</template>
	
	<template name="Noptions_checkbox_state">
		<widgetstate statename="{state}" float="right" padding="3%">
			<panel width="{size}" color="invisible">
				<frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="0.5h" color="white"/>
				<frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="0.5h"/>
				<image x="0.15h" y="0.15h" width="-0.3h" height="-0.3h" texture="/ui/elements/check.tga" visible="{checked}" />
			</panel>
			<label width="-{size}" height="100%" textvalign="center" font="{font}" color="{color}" shadow="true">{label}</label>
		</widgetstate>
	</template>
	
	<template name="Noptions_checkbox">
		<panel width="{width=90%}" height="{height=1.5h}" align="{align=center}" valign="{valign=top}" noclick="true" color="invisible">
			<button 
				color="invisible"
				numstates="2"
				cvar="{cvar=}"
				onclick="PlaySound('/shared/sounds/ui/button_click.mp3'); {onclick=}"
				onmouseover="If(!StringEmpty('{tip_id}'), ShowWidget('{tip_id}')); PlaySound('/shared/sounds/ui/ccpanel/button_over_01.wav');"
				onmouseout="If(!StringEmpty('{tip_id}'), HideWidget('{tip_id}'));"
				watch="{watch=}"
				ontrigger="{ontrigger=}"
			>
				<instance name="Noptions_checkbox_state" state="up" size="{height=1.5h}" label="{label}" font="{font=dyn_9}" color="white" checked="false"/>
				<instance name="Noptions_checkbox_state" state="over" size="{height=1.5h}" label="{label}" font="{font=dyn_9}" color="#3abde7" checked="false"/>
				<instance name="Noptions_checkbox_state" state="down" size="{height=1.5h}" label="{label}" font="{font=dyn_9}" color="#3abde7" checked="false"/>
				<instance name="Noptions_checkbox_state" state="up2" size="{height=1.5h}" label="{label}" font="{font=dyn_9}" color="#ffaa00" checked="true"/>
				<instance name="Noptions_checkbox_state" state="over2" size="{height=1.5h}" label="{label}" font="{font=dyn_9}" color="#3abde7" checked="true"/>
				<instance name="Noptions_checkbox_state" state="down2" size="{height=1.5h}" label="{label}" font="{font=dyn_9}" color="#3abde7" checked="true"/>
			</button>
		</panel>
	</template>
	
	<template name="more_options">
		<button height="1.5h" valign="center" numstates="2" color="invisible" onclick="ToggleWidget('{id}_options');" onrightclick="ToggleWidget('{id}_options');">					
			<widgetstate statename="up1" float="right" padding="3%">				
				<panel width="1.5h" color="invisible">
					<frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="0.5h" color="white"/>
					<frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="0.5h"/>
				</panel>
				<label width="-1.5h" height="100%" textvalign="center" font="dyn_9" color="1 1 1 1" shadow="true">{display_label}</label>
			</widgetstate>			
			<widgetstate statename="over1" float="right" padding="3%">
				<panel width="1.5h" color="invisible">
					<frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="0.5h" color="white"/>
					<frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="0.5h"/>
				</panel>
				<label width="-1.5h" height="100%" textvalign="center" font="dyn_9" color="#3abde7" shadow="true">{display_label}</label>
			</widgetstate>			
			<widgetstate statename="down1" float="right" padding="3%">
				<panel width="1.5h" color="invisible">
					<frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="0.5h" color="white"/>
					<frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="0.5h"/>
				</panel>
				<label width="-1.5h" height="100%" textvalign="center" font="dyn_9" color="#3abde7" shadow="true">{display_label}</label>
			</widgetstate>			
			<widgetstate statename="up2" float="right" padding="3%">
				<panel width="1.5h" color="invisible">
					<frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="0.5h" color="white"/>
					<frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="0.5h"/>
					<image x="0.15h" y="0.15h" width="-0.3h" height="-0.3h" texture="/ui/elements/check.tga"/>
				</panel>
				<label width="-1.5h" height="100%" textvalign="center" font="dyn_9" color="#ffaa00" shadow="true">{display_label}</label>
			</widgetstate>			
			<widgetstate statename="over2" float="right" padding="3%">
				<panel width="1.5h" color="invisible">
					<frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="0.5h" color="white"/>
					<frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="0.5h"/>
					<image x="0.15h" y="0.15h" width="-0.3h" height="-0.3h" texture="/ui/elements/check.tga"/>
				</panel>
				<label width="-1.5h" height="100%" textvalign="center" font="dyn_9" color="#3abde7" shadow="true">{display_label}</label>
			</widgetstate>			
			<widgetstate statename="down2" float="right" padding="3%">
				<panel width="1.5h" color="invisible">
					<frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="0.5h" color="white"/>
					<frame texture="/ui/frames/roundframe.tga" color="#3abde7" borderthickness="0.5h"/>
					<image x="0.15h" y="0.15h" width="-0.3h" height="-0.3h" texture="/ui/elements/check.tga"/>
				</panel>
				<label width="-1.5h" height="100%" textvalign="center" font="dyn_9" color="#3abde7" shadow="true">{display_label}</label>
			</widgetstate>			
		</button>
	</template>
	
	<template name="field">
		<frame texture="/ui/frames/rounded_bg_white.tga" borderthickness="0.5h" color="#171a28"/>
		<frame texture="/ui/frames/roundframe.tga" color="#bbdefa" borderthickness="0.5h"/>
		<textbox
			name="{widgetname}"
			taborder="{order}"
			passwordchar="{passchar=}"
			maxlength="{maxlength=-1}"
			y="0.3h"
			width="-1.0h"
			align="center"
			color="invisible"
			font="dyn_9"
			textcolor="white"
			shadow="true"
			onchange="{onchange}"
			onshow="{onshow}"
			onfocus="{onfocus}"
			onlosefocus="{onlosefocus}"
			form="{form=}"
			data="{data=}"
		/>		
	</template>
	
	<template name="tab_bar">
		<panel x="{x}" y="{y}" width="{width=100%}" height="{height=100%}" align="{align}" valign="{valign}" color="invisible" noclick="true" passivechildren="true">
			<image width="-64" align="center" texture="/ui/info/tab_bar_c.tga" color="{color=#ffffff}"/>
			<image height="50.1%" width="32" align="left" valign="top" texture="/ui/info/tab_bar_tl.tga" color="{color=#ffffff}"/>
			<image height="49.9%" width="32" align="left" valign="bottom" texture="/ui/info/tab_bar_bl.tga" color="{color=#ffffff}"/>
			<image height="50.1%" width="32" align="right" valign="top" texture="/ui/info/tab_bar_tr.tga" color="{color=#ffffff}"/>
			<image height="49.9%" width="32" align="right" valign="bottom" texture="/ui/info/tab_bar_br.tga" color="{color=#ffffff}"/>
		</panel>
	</template>

	<template name="header_bar">
		<panel group="{group}" x="{x}" y="{y}" width="{width=100%}" height="{height=100%}" align="{align}" valign="{valign}" color="invisible" noclick="{noclick=1}" blockinput="0">
			<image width="-64" align="center" texture="/ui/info/header_bar_c.tga" color="{color=#ffffff}" noclick="1" blockinput="0"/>
			<image height="50.1%" width="32" align="left" valign="top" texture="/ui/info/header_bar_tl.tga" color="{color=#ffffff}" noclick="1" blockinput="0"/>
			<image height="49.9%" width="32" align="left" valign="bottom" texture="/ui/info/header_bar_bl.tga" color="{color=#ffffff}" noclick="1" blockinput="0"/>
			<image height="50.1%" width="32" align="right" valign="top" texture="/ui/info/header_bar_tr.tga" color="{color=#ffffff}" noclick="1" blockinput="0"/>
			<image height="49.9%" width="32" align="right" valign="bottom" texture="/ui/info/header_bar_br.tga" color="{color=#ffffff}" noclick="1" blockinput="0"/>
		
			<label name="{label_name}" x="6" y="-1" width="100%" height="100%" font="{lbl_font=dyn_10}" textvalign="center" color="{lbl_color=white}" watch="{watch}" ontrigger="{ontrigger}" noclick="1" blockinput="0">{lbl}</label>
			<button
				visible="{show_close}"
				x="-6" y="-1"			
				width="{buttonsize=1.8h}" height="{buttonsize=1.8h}"
				align="right"
				valign="center"
				color="1 1 1 1"
				texture="/ui/info/close_button.tga"
				onclick="PlaySound('/shared/sounds/ui/ccpanel/button_close_02.wav'); {onclick}"
				onmouseover="PlaySound('/shared/sounds/ui/ccpanel/button_over_01.wav'); {onmouseover}"
			/>			
		</panel>
	</template>

	<template name="button">
		<panel x="{x=}" y="{y=}" width="{width=100%}" height="{height=100%}" align="{align=}" valign="{valign=}" noclick="true">
			<panel color="1 0 0 0" width="220%" height="400%" valign="center" align="center" visible="{effect=1}" noclick="true">
				<modelpanel 
					name="{id}"
					fog="true"
					fogcolor="0 0 0" fognear="500" fogfar="600" fogscale="1"
					noclick="true"						
					cameraangles="0.0 0.0 180.0"
					camerapos="0.0 500.0 0.0"
					camerafov="90.0"
					camerafar="600.0"
					model0="/ui/effects/nebula/button_smoke.mdf"
					anim0="start"
					depthclear="false"
					depthcompress="true"
				/>				
			</panel>		

			<button
				group="{group}"
				name="{button_name}"
				color="invisible"
				visible="{visible=true}"
				watch="{watch}"
				enabled="{enabled=true}"
				ontrigger="{ontrigger}"
				onmouseover="PlaySound('/shared/sounds/ui/button_over_01.wav'); Call('{id}', 'SetAnim(0, \'start\');');   Call('{id}',  'SetEffect(0, \'/ui/effects/nebula/button_smoke.effect\');'); {onmouseover}"
				onmouseout="{onmouseout}"
				onclick="PlaySound('{sound=/shared/sounds/ui/button_click_01.wav}'); {onclick}"
				onenter="PlaySound('{sound='/shared/sounds/ui/button_click_01.wav}'); {onenter}"
				onshow="{onshow}"
				hotkey="{hotkey}"
				onhotkey="PlaySound('{sound=/shared/sounds/ui/button_click_01.wav'}'); {onhotkey}"
			>
				<widgetstate statename="up">
					<frame texture="/ui/info/button.tga" borderthickness="{borderthickness=12}" color="1 1 1 1"/>
					<!--<frame texture="/ui/frames/roundframe.tga" color="gray" borderthickness="8"/>-->				
					<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="{font}" color="white" shadow="true" shadowcolor="black">{label}</label>
				</widgetstate>
				
			
			
				<widgetstate statename="over">
					<frame texture="/ui/info/button_over.tga" borderthickness="{borderthickness=12}" />				
					<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="{font}" color="white" shadow="true" shadowcolor="#7F6E00">{label}</label>
				</widgetstate>
				
				
				<widgetstate statename="down" x="1" y="1">
					<frame texture="/ui/info/button_down.tga" borderthickness="{borderthickness=12}" color="white"/>
					
					<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="{font}" color="#3f0001" shadow="true" shadowcolor="#7F6E00">{label}</label>
				</widgetstate>
				<widgetstate statename="disabled">
					<frame texture="/ui/info/button.tga" borderthickness="{borderthickness=8}" color="1 1 1 1"/>	
					<frame texture="/ui/frames/rounded_button_bg_white.tga" borderthickness="8" color=".3 .3 .3 .7"/>
					<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="{font}" color="#494c5f" shadow="true" shadowcolor="black">{label}</label>
				</widgetstate>
			</button>
		</panel>
	</template>

	<template name="button3">
		<panel x="{x=}" y="{y=}" width="{width=100%}" height="{height=100%}" align="{align=}" valign="{valign=}" noclick="true">
			<button
				group="{group}"
				name="{button_name}"
				color="invisible"				
				visible="{visible=true}"
				watch="{watch}"
				enabled="{enabled=true}"
				ontrigger="{ontrigger}"
				onmouseover="PlaySound('/shared/sounds/ui/menu/button_over.wav'); {onmouseover}"
				onmouseout="{onmouseout}"
				onclick="PlaySound('{sound=/shared/sounds/ui/menu/button_click.wav}'); {onclick}"
				onenter="{onenter}"
				onshow="{onshow}"
				onframe="{onframe}"
				hotkey="{hotkey}"
				onhotkey="{onhotkey}"
				onenable="{onenable}"
			>
				<widgetstate statename="up">
					<frame texture="/ui/info/button.tga" borderthickness="{borderthickness=0.8h}" color="1 1 1 1"/>
					<!--<frame texture="/ui/frames/roundframe.tga" color="gray" borderthickness="8"/>-->				
					<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="{font}" color="white" shadow="true" shadowcolor="black">{label}</label>
				</widgetstate>

				<widgetstate statename="over">
					<frame texture="/ui/info/button_over.tga" borderthickness="{borderthickness=0.8h}" />				
					<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="{font}" color="white" shadow="true" shadowcolor="#7F6E00">{label}</label>
				</widgetstate>

				<widgetstate statename="down" x="1" y="1">
					<frame texture="/ui/info/button_down.tga" borderthickness="{borderthickness=0.8h}" color="white"/>
					<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="{font}" color="#3f0001" shadow="true" shadowcolor="#7F6E00">{label}</label>
