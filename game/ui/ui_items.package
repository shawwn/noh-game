			</panel>
	
			<label x="8" y="61" width="100%" font="dyn_10" color="white" shadow="true" watch="ChatHoverServer" ontrigger="SetText(Translate('ui_items_friend_server_label') # param);"/>
			<!-- label x="8" y="76" font="dyn_10" color="white" shadow="true">^yTeam: ^*{team}</label -->
			<label x="8" y="76" width="100%" font="dyn_10" color="white" shadow="true" watch="ChatHoverGameTime" ontrigger="SetText(Translate('ui_items_friend_game_time_label') # param);"/>
			
			<label x="0" y="93" width="100%" font="dyn_8" color="white" shadow="true" textalign="center">ui_items_friend_right_click_label</label>
	
		</panel>
	</template>
	
	<template name="player_voice_button">
		<button
			name="{id}"
			group="{group}"
			x="{x}" y="{y}"
			width="{width=100%}" height="{height=100%}"
			align="{align}" valign="{valign=center}"
			color="invisible"
			onmouseover="PlaySound('/shared/sounds/ui/button_over_02.wav'); {onmouseover}"
			onmouseout="{onmouseout}"
			onclick="PlaySound('/shared/sounds/ui/button_click_05.wav'); ToggleVoiceMute(*(name # '_voice_client_num')); if(IsVoiceMuted(*(name # '_voice_client_num')), SetButtonState(2), if(*(name # '_voice_talking'), SetButtonState(1), SetButtonState(0)));"
			numstates="3"
			visible="{visible=true}"
			onshow="if(GetLocalTeam() != {team} and !({alwaysvisible=false}), SetVisible(false)); SetButtonState(0);"
			watch="VoiceChatTalking{teamindex}"
			ontrigger="SetEnabled(param3 != GetLocalClientNum()); SetVisible({alwaysvisible=false} or GetLocalTeam() == param4); CreateInt(name # '_voice_client_num', param3); CreateBool(name # '_voice_talking', param0); if(IsVoiceMuted(param3), SetButtonState(2), if(param0, SetButtonState(1), SetButtonState(0)));"
		>
			<widgetstate statename="up">
				<image texture="{texture}" />
			</widgetstate>

			<widgetstate statename="over">
				<image texture="{texture}" />
			</widgetstate>

			<widgetstate statename="down" x="1" y="1">
				<image texture="{texture}" />
			</widgetstate>

			<widgetstate statename="disabled">
				<image texture="{texture}" color=".5 .5 .5 1"/>
			</widgetstate>

			<widgetstate statename="up2">
				<image texture="{texture}" color=".0 .8 .0 1"/>
			</widgetstate>

			<widgetstate statename="over2">
				<image texture="{texture}" color=".0 .8 .0 1"/>
			</widgetstate>

			<widgetstate statename="down2" x="1" y="1">
				<image texture="{texture}" color=".0 .8 .0 1"/>
			</widgetstate>
			
			<widgetstate statename="up3">
				<image texture="{texture}" color=".8 .0 .0 1"/>
			</widgetstate>

			<widgetstate statename="over3">
				<image texture="{texture}" color=".8 .0 .0 1"/>
			</widgetstate>

			<widgetstate statename="down3" x="1" y="1">
				<image texture="{texture}" color=".8 .0 .0 1"/>
			</widgetstate>
		</button>
	</template>

	<!-- Round button template -->
	<template name="round_button2">
		<button
			group="{group}"
			name="{button_name}"
			x="{x}" y="{y}"
			width="{width=100%}" height="{height=100%}"
			align="{align}" valign="{valign}"
			color="invisible"
			visible="{visible=true}"
			watch="{watch}"
			enabled="{enabled=true}"
			ontrigger="{ontrigger}"
			onmouseover="{onmouseover}"
			onmouseout="{onmouseout}"
			onclick="{onclick}"
			onenter="{onenter}"
			onshow="{onshow}"
			hotkey="{hotkey}"
			onhotkey="{onhotkey}"
		>
			<widgetstate statename="up">
				<frame texture="/ui/frames/rounded_button_bg_white.tga" borderthickness="0.7h" color="white"/>
				<frame texture="/ui/frames/roundframe.tga" color="gray" borderthickness="0.7h"/>
				<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="{font}" color="black" shadow="true" shadowcolor="gray">{label}</label>
			</widgetstate>
			<widgetstate statename="over">
				<frame texture="/ui/frames/rounded_button_bg_white.tga" borderthickness="0.7h" color="#FFE333"/>
				<frame texture="/ui/frames/roundframe.tga" color="#7F6E00" borderthickness="0.7h"/>
				<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="{font}" color="white" shadow="true" shadowcolor="#7F6E00">{label}</label>
			</widgetstate>
			<widgetstate statename="down" x="1" y="1">
				<frame texture="/ui/frames/rounded_button_bg_white.tga" borderthickness="0.7h" color="#FFE333"/>
				<frame texture="/ui/frames/roundframe.tga" color="#7F6E00" borderthickness="0.7h"/>
				<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="{font}" color="#3f0001" shadow="true" shadowcolor="#7F6E00">{label}</label>
			</widgetstate>
			<widgetstate statename="disabled">
				<frame texture="/ui/frames/rounded_button_bg_white.tga" borderthickness="0.7h" color="#666666"/>
				<frame texture="/ui/frames/roundframe.tga" color="#666666" borderthickness="0.7h"/>
				<label width="100%" height="100%" textvalign="center" textalign="center" valign="center" align="center" font="{font}" color="#333333" shadow="true" shadowcolor="black">{label}</label>
			</widgetstate>
		</button>
	</template>
	
	<!-- Alert Box -->
	<template name="alert_box">
		<panel width="{width}" height="{height}" align="{align=center}" valign="{valign=center}" x="{x}" y="{y}" noclick="1">
		<frame color="0 0 0 .8" bordercolor="#455166" borderthickness="1" noclick="1" />		
			<panel y="-0.5h" width="-1.0h" height="-3.4h" align="center" valign="bottom" noclick="1">
				<frame texture="/ui/frames/tooltip_bg.tga" color="#2c4b6c" noclick="1"/>
				<frame color="invisible" bordercolor="#455166" borderthickness="1" noclick="1" />				
				<panel width="-1.0h" height="-1.0h" align="center" valign="center" color="invisible" noclick="1">				
					<instance name="{body}"/>				
				</panel>				
			</panel>			
			<instance
				name="header_bar"
				buttonsize="1.8h"
				height="2.4h"
				width="-2"
				y="1"
				align="center"
				lbl="{label}"
				show_close="{show_close=false}"
				onclick="PlaySound('/shared/sounds/ui/button_click.mp3'); {closebtn_click}');"
			/>			
		</panel>
	</template>
	
	<template name="recently_played_listbox">
		<panel name="{listbox_name}_panel" width="-1.0h" height="-1.0h" align="center" valign="center" onload="CreateInt('{listbox_name}_num_expanded', 0);">
			<frame texture="/ui/info/backdrop.tga" borderthickness="2.0h" color="1 1 1 1" noclick="1" />
			<listbox
				name="{listbox_name}_listbox"
				visible="true"
				align="center" valign="top"
				color="invisible"
				font="dyn_10"					
				itemwidth="100%"
				itemheight="2.2h"					
				select="true"
				hoverselect="false"
				highlight="under"
				highlightcolor="#67a7da33"
				highlightbordercolor="invisible"
				exteriorscrollbars="false"
				scrollbarplaceholder="false"
				scrolltexture="/ui/info/combobox.tga"
				scrollbarsize="2.0h" 
				handleheight="2.0h"
				clearselection="true"
				ondoubleclick="{ondoubleclick}"
				watch1="ChatRecentlyPlayedPlayer"
				ontrigger1="AddTemplateListItemWithSort('cc_recently_played_player', param0, param1, 'name', param0, 'tooltip', '{tooltip}');"
				watch2="ChatRecentlyPlayedHeader"
				ontrigger2="AddTemplateListItemWithSort('cc_recently_played_header', param0, param0, 'matchid', param0, 'time', param1, 'gamename', param2, 'select', false);"
				watch3="ChatRecentlyPlayedEvent"
				ontrigger3="(*param)();"
				onselect="Set('ui_lastSelectedUser', this);"
				onhover="ShowWidget('hover_tip'); if(!ChatUpdateHoverInfo(this), HideWidget('hover_tip'));"
			/>
		</panel>
	</template>
	
	<template name="cc_recently_played_player">
		<panel passivechildren="true"
			onrightclick="
				Set('ui_lastSelectedUser', '{name}');
				Call('{tooltip}', 'SetVisible(true);');
				Call('{tooltip}', 'SetAbsoluteX(\'' # GetMouseX() # '\');');
				Call('{tooltip}', 'SetAbsoluteY(\'' # GetMouseY() # '\');');
				Call('{tooltip}_choices', 'ListBoxCmd(\'SelectNone\');');
				PlaySound('/shared/sounds/ui/button_rightclick_01.mp3');
				SetFocusName('{tooltip}');">
				
			<label width="-1.6h" textvalign="center" textalign="left" align="center" valign="center" font="dyn_9" color="#80878d" shadow="1" wrap="1">{name}</label>
		</panel>
	</template>
	
	<template name="cc_recently_played_header">
		<instance name="header_bar" color="#b3a4a4" />
		<frame color="invisible" bordercolor="#697a9748" borderthickness="2" noclick="1" />
		<frame color="invisible" bordercolor="0 0 0 .8" borderthickness="1" noclick="1" />
		
		<label width="44%" x="1%" height="-6%" textvalign="center" textalign="left" align="left" valign="center" font="dyn_10" color="white" shadow="1" wrap="false">{gamename}</label>
		<label width="54%" x="-1%" height="-6%" textvalign="center" textalign="right" align="right" valign="center" font="dyn_10" color="white" shadow="1" wrap="false">{matchid}, {time}</label>
	</template>
	
	<template name="autocomplete_list">
		<panel name="{listbox_name}_panel" y="{y=0}" x="{x=0}" width="{width=-1.0h}" height="{height=-1.0h}" align="{align=center}" valign="{valign=center}">
			<frame texture="/ui/info/backdrop.tga" borderthickness="1.6h" color="1 1 1 1" noclick="1" />
			<listbox
				name="{listbox_name}_listbox"
				visible="true"
				align="center" valign="top"
				color="invisible"
				font="dyn_10"
				itemwidth="100%"
				itemheight="2.2h"
				select="true"
				hoverselect="true"
				highlight="under"
				highlightcolor="#67a7da33"
				highlightbordercolor="invisible"
				exteriorscrollbars="false"
				scrollbarplaceholder="false"
				scrolltexture="/ui/info/combobox.tga"
				scrollbarsize="2.0h" 
				handleheight="2.0h"
				clearselection="true"
				onclick="{onclick}"
				ondoubleclick="{ondoubleclick}"
				watch1="ChatAutoCompleteClear"
				ontrigger1="ClearItems();"
				watch2="ChatAutoCompleteAdd"
				ontrigger2="AddTemplateListItem('autocomplete_player', param0, 'name', param0);"
				onselect="Set('ui_lastSelectedUser', this); Call(ui_autocompleteTextBox, 'SetInputLine(ui_lastSelectedUser);'); ChatAutoCompleteClear();"
			/>
		</panel>
	</template>
	
	<template name="autocomplete_player">
		<label width="-1.6h" textvalign="center" textalign="left" align="center" valign="center" font="dyn_9" color="1 1 1 1" shadow="1" wrap="1">{name}</label>
	</template>
</package>