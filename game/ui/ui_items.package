	<template name="buddy_listbox">
		<panel name="{listbox_name}_panel" width="-1.0h" height="-1.0h" align="center" valign="center" onload="CreateInt('{listbox_name}_num_expanded', 0);">
			<frame texture="/ui/info/backdrop.tga" borderthickness="2.0h" color="1 1 1 1" noclick="1" />
			<listbox
				name="{listbox_name}_listbox"
				visible="true"
				align="center" valign="top"
				color="invisible"
				font="dyn_10"					
				itemwidth="100%"
				itemheight="2.2h"					
				select="true"
				hoverselect="false"
				highlight="under"
				highlightcolor="#67a7da33"
				highlightbordercolor="invisible"
				exteriorscrollbars="false"
				scrollbarplaceholder="false"
				scrolltexture="/ui/info/combobox.tga"
				scrollbarsize="2.0h" 
				handleheight="2.0h"
				clearselection="true"
				ondoubleclick="{ondoubleclick}"
				watch1="ChatBuddyOnline"
				ontrigger1="AddTemplateListItemWithSort('cc_friend_online', param, '1' # param, 'name', param, 'tooltip', '{tooltip}');"
				watch2="ChatBuddyGame"
				ontrigger2="AddTemplateListItemWithSort('cc_friend_game', param, '3' # param, 'name', param, 'tooltip', '{tooltip}');"
				watch3="ChatBuddyOffline"
				ontrigger3="AddTemplateListItemWithSort('cc_friend_offline', param, '5' # param, 'name', param, 'tooltip', '{tooltip}');"
				watch4="ChatBuddyEvent"
				ontrigger4="
					(*param)();
					If(StringEquals(param, 'ClearItems'),
						Split(
							AddTemplateListItemWithSort('cc_friend_header', '0', '0', 'label', 'ui_items_cc_friends_online', 'select', false),
							AddTemplateListItemWithSort('cc_friend_header', '2', '2', 'label', 'ui_items_cc_friends_in_game', 'select', false),
							AddTemplateListItemWithSort('cc_friend_header', '4', '4', 'label', 'ui_items_cc_friends_offline', 'select', false)));"
				onselect="Set('ui_lastSelectedUser', this);"
				onhover="ShowWidget('hover_tip'); if(!ChatUpdateHoverInfo(this), HideWidget('hover_tip'));"
			/>
		</panel>
	</template>
	
